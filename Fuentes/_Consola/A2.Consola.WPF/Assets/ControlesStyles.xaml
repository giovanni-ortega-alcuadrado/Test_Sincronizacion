                                                    Opacity="0"
                                                    RadiusX="0"
                                                    RadiusY="0"
                                                    Stroke="#00000000"
                                                    StrokeThickness="{TemplateBinding BorderThickness}" />
                                                <Rectangle
                                                    x:Name="BackgroundGradient"
                                                    Margin="{TemplateBinding BorderThickness}"
                                                    Fill="{TemplateBinding Background}"
                                                    RadiusX="0"
                                                    RadiusY="0"
                                                    Stroke="#FFFFFFFF"
                                                    StrokeThickness="0" />
                                                <Rectangle
                                                    x:Name="BackgroundOverlay3"
                                                    Fill="{TemplateBinding Background}"
