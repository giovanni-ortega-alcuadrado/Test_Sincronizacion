                            x:Name="FocusVisualElement"
                            Margin="1"
                            IsHitTestVisible="false"
                            Opacity="0"
                            RadiusX="2"
                            RadiusY="2"
                            Stroke="#FF6DBDD1"
                            StrokeThickness="1" />
