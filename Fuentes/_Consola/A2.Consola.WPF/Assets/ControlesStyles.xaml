                                                    RadiusX="30"
                                                    RadiusY="0"
                                                    Stroke="#00000000"
                                                    StrokeThickness="{TemplateBinding BorderThickness}" />
                                                <Rectangle
                                                    x:Name="BackgroundGradient2"
                                                    Margin="{TemplateBinding BorderThickness}"
                                                    Fill="{TemplateBinding Background}"
                                                    Opacity="0"
                                                    RadiusX="0"
                                                    RadiusY="0"
                                                    Stroke="#FFFFFFFF"
                                                    StrokeThickness="0" />
                                                <Rectangle
