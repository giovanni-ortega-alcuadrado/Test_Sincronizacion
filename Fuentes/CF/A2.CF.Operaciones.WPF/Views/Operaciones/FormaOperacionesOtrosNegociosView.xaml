                                        Maximum="999999999999999"
                                        Minimum="0"
                                        NumberDecimalDigits="{Binding FormatoCamposDecimales, Source={StaticResource VMOperaciones}}"
                                        ShowButtons="False"
                                        ValueFormat="Numeric"
                                        Value="{Binding IVA, Mode=TwoWay}" />
                                </Telerik:DataFormDataField>

                                <Telerik:DataFormDataField
                                    Grid.Row="1"
                                    Grid.Column="3"
                                    Label="Valor neto COP"
                                    LabelPosition="Above">
                                    <A2UTILGEN:A2NumericBox
                                        x:Name="txtValorNetoCopDivisas"
                                        AllowNull="False"
                                        FuncionalidadAutocomplete="True"
                                        GotFocus="SeleccionarFocoControl"
                                        IsEnabled="False"
                                        Maximum="999999999999999"
                                        Minimum="-999999999999999"
                                        NumberDecimalDigits="{Binding FormatoCamposDecimalesEspeciales, Source={StaticResource VMOperaciones}}"
                                        ShowButtons="False"
                                        ValueFormat="Numeric"
                                        Value="{Binding ValorNetoCop, Mode=TwoWay}" />
                                </Telerik:DataFormDataField>
                            </Grid>

                            <Grid Grid.Row="1">
                                <Telerik:DataFormDataField Label="Observaciones" LabelPosition="Above">
                                    <TextBox
                                        x:Name="txtObservaciones"
                                        GotFocus="SeleccionarFocoControl"
                                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                                        MaxLength="255"
                                        Text="{Binding Observaciones, Mode=TwoWay}"
                                        TextWrapping="Wrap" />
                                </Telerik:DataFormDataField>
                            </Grid>
                        </Grid>
                    </StackPanel>

                </TabItem>

                <!--  Tab para la distribución de comisiones  -->
                <TabItem x:Name="tabItemDistribucionComisiones" Header="Distribución de comisiones">
                    <StackPanel
                        x:Name="stackDistribucionComisiones"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch">
                        <A2:ControlMenuDetalleA2
                            x:Name="cmReceptoresOperaciones"
                            HorizontalAlignment="Right"
                            IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                            Mensaje="{Binding MensajeMVVM, Source={StaticResource VMOperaciones}, Mode=TwoWay}"
                            NombreDetalle="{Binding NombreColeccionDetalle, Source={StaticResource VMOperaciones}, Mode=TwoWay}"
                            ObjetoSeleccionado="{Binding ReceptorSeleccionado, Source={StaticResource VMOperaciones}}"
                            PreguntarConfirmacion="False" />
                        <A2UTILGEN:A2GridView
                            x:Name="dgReceptoresReceptores"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            AutoGenerateColumns="False"
                            CanUserDeleteRows="False"
                            CanUserInsertRows="False"
                            IsHitTestVisible="True"
                            ItemsSource="{Binding ListaReceptores, Source={StaticResource VMOperaciones}}"
                            ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                            ScrollViewer.VerticalScrollBarVisibility="Hidden"
                            SelectedItem="{Binding ReceptorSeleccionado, Source={StaticResource VMOperaciones}, Mode=TwoWay}"
                            ValidatesOnDataErrors="None">
                            <A2UTILGEN:A2GridView.Columns>
                                <Telerik:GridViewDataColumn Width="Auto" Header="">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <Controles:BuscadorGenericoListaButon
                                                Agrupamiento=""
                                                CampoBusqueda="DetalleReceptor"
                                                EstadoItem="A"
                                                Etiqueta="Receptor"
                                                IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                                                TipoItem="Receptores"
                                                finalizoBusqueda="BuscadorGenericoListaButon_finalizoBusqueda" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn
                                    DataMemberBinding="{Binding Receptor, Mode=TwoWay}"
                                    Header="Código asesor"
                                    IsReadOnly="True" />
                                <Telerik:GridViewDataColumn
                                    DataMemberBinding="{Binding NombreReceptor}"
                                    Header="Nombre"
                                    IsReadOnly="True" />
                                <Telerik:GridViewDataColumn Width="Auto" Header="Lider">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox
                                                Margin="30,0,0,0"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                IsChecked="{Binding Lider, Mode=TwoWay}"
                                                IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn Width="Auto" Header="Porcentaje">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <A2UTILGEN:A2NumericBox
                                                HorizontalAlignment="Stretch"
                                                VerticalAlignment="Stretch"
                                                IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                                                Maximum="100"
                                                Minimum="0"
                                                ShowButtons="False"
                                                Value="{Binding Porcentaje, Mode=TwoWay}" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>

                                <Telerik:GridViewDataColumn Width="Auto">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <Label VerticalAlignment="Center" Content="%" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                            </A2UTILGEN:A2GridView.Columns>
                        </A2UTILGEN:A2GridView>
                    </StackPanel>
                </TabItem>

            </TabControl>
        </Border>
    </Grid>

</UserControl>
