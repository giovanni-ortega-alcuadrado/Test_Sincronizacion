<UserControl
    x:Class="FormaOperacionesOtrosNegociosView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:A2="clr-namespace:A2ControlMenu;assembly=A2ControlMenu.WPF"
    xmlns:A2UTILGEN="clr-namespace:A2Utilidades;assembly=A2.Controles.Utilidades.WPF"
    xmlns:Controles="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:OYDPLUSControles="clr-namespace:A2OYDPLUSUtilidades;assembly=A2.OyD.PLUSUtilidades.WPF"
    xmlns:Telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:src="clr-namespace:A2CFOperaciones"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    mc:Ignorable="d">

    <UserControl.Resources>
        <src:DisplayNameConverter x:Key="DisplayConverter" />
        <src:VisibilityConverter x:Key="VisibilityConverter" />
        <src:InVisibilityConverter x:Key="InVisibilityConverter" />
        <src:BooleanVisibilityConverter x:Key="BooleanVisibilityConverter" />
        <src:StringConverter x:Key="StringConverter" />
        <src:clsLiderConverter x:Key="cvLider" />
        <src:LongConverter x:Key="cvCodigo" />
        <src:DateTimeHoraFinDiaConverter x:Key="cvDateTimeHoraFinDiaConverter" />
        <src:BooleanInvertidoConverter x:Key="BooleanInvertidoConverter" />
    </UserControl.Resources>

    <Grid
        x:Name="LayoutRoot"
        HorizontalAlignment="Stretch"
        DataContext="{Binding EncabezadoSeleccionado, Source={StaticResource VMOperaciones}}">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <Border
            Grid.Row="1"
            Grid.Column="0"
            Margin="0,2,0,2"
            HorizontalAlignment="Stretch"
            Style="{StaticResource BorderFormSup}">
            <Grid x:Name="GridControlesIniciales">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <Telerik:DataFormDataField
                    Grid.Row="0"
                    Grid.Column="0"
                    Label="Número operación"
                    LabelPosition="Above">
                    <!--<StackPanel Grid.Row="0" Grid.Column="0"
                            Orientation="Vertical"
                            HorizontalAlignment="Stretch"
                            Margin="2">
                    <Label Content=""
                                   Style="{StaticResource EstiloLabel}"/>-->
                    <TextBlock
                        x:Name="txtIDOperacion"
                        Style="{StaticResource ControlTextBlockNormalInfo}"
                        Text="{Binding ID, Mode=TwoWay}" />
                    <!--</StackPanel>-->
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="0"
                    Grid.Column="1"
                    Label="Estado"
                    LabelPosition="Above">

                    <TextBlock
                        x:Name="txtEstado"
                        Style="{StaticResource ControlTextBlockNormalInfo}"
                        Text="{Binding NombreEstado, Mode=TwoWay}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="0"
                    Grid.Column="2"
                    Label="Tipo registro"
                    LabelPosition="Above">

                    <TextBlock
                        x:Name="txtTipoRegistro"
                        Style="{StaticResource ControlTextBlockNormalInfo}"
                        Text="{Binding NombreTipoRegistro, Mode=TwoWay}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="0"
                    Grid.Column="3"
                    Label="Usuario registro"
                    LabelPosition="Above">
                    <TextBlock
                        x:Name="txtUsuarioRegistro"
                        Style="{StaticResource ControlTextBlockNormalInfo}"
                        Text="{Binding UsuarioRegistro, Mode=TwoWay}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="0"
                    Grid.Column="4"
                    Label="Fecha registro"
                    LabelPosition="Above">
                    <TextBlock
                        x:Name="txtFechaRegistro"
                        Style="{StaticResource ControlTextBlockNormalInfo}"
                        Text="{Binding FechaRegistro, StringFormat='dd/MM/yyyy hh:mm:ss tt', Mode=TwoWay}" />
                </Telerik:DataFormDataField>


                <StackPanel
                    Grid.Row="0"
                    Grid.Column="8"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center"
                    Orientation="Horizontal">

                    <Button x:Name="btnAplazar"
                            Click="btnAplazar_Click"
                            Content="Aplazar"
                            IsEnabled="{Binding HabilitaBoton, Source={StaticResource VMOperaciones}}"
                            Style="{StaticResource ControlButtonSecundaryStyle}"
                            ToolTipService.ToolTip="Aplazar" />

                    <Button x:Name="btnDuplicar"
                            Click="btnDuplicar_Click"
                            Content="Duplicar"
                            IsEnabled="{Binding HabilitaBoton, Source={StaticResource VMOperaciones}}"
                            Style="{StaticResource ControlButtonSecundaryStyle}"
                            ToolTipService.ToolTip="Duplicar"/>

                    <Button x:Name="btnLiqxDife"
                            Click="btnLiqxDife_Click"
                            Content="LiqxDife"
                            IsEnabled="{Binding HabilitaBoton, Source={StaticResource VMOperaciones}}"
                            Style="{StaticResource ControlButtonSecundaryStyle}"
                            ToolTipService.ToolTip="LiqxDife" />
                </StackPanel>

                
                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="0"
                    Label="Tipo negocio"
                    LabelPosition="Above">
                    <!--<StackPanel Grid.Row="1"
                            Grid.Column="0"
                            Margin="2"
                            HorizontalAlignment="Stretch"
                            Orientation="Vertical">
                    <Label Content="" Style="{StaticResource EstiloLabel}" />-->
                    <ComboBox
                        x:Name="cboTipoNegocio"
                        DisplayMemberPath="NombreTipoNegocio"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARENCABEZADO], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding ListaTipoNegocio, Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding TipoNegocio, Mode=TwoWay}"
                        SelectedValuePath="CodigoTipoNegocio" />
                    <!--</StackPanel>-->
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="1"
                    Label="Tipo origen"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboTipoOrigen"
                        DisplayMemberPath="NombreTipoNegocio"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARENCABEZADO], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding ListaTipoOrigen, Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding TipoOrigen, Mode=TwoWay}"
                        SelectedValuePath="CodigoTipoNegocio" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="2"
                    Label="Tipo operación"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboTipoOperacion"
                        DisplayMemberPath="Descripcion"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARENCABEZADO], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding DiccionarioCombos[NEGOCIOS_TIPOOPERACION], Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding TipoOperacion, Mode=TwoWay}"
                        SelectedValuePath="Codigo" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="3"
                    Label="Nro Referencia"
                    LabelPosition="Above">

                    <TextBox
                        x:Name="txtReferencia"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARENCABEZADO], Source={StaticResource VMOperaciones}}"
                        MaxLength="15"
                        Text="{Binding Referencia, Mode=TwoWay}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="4"
                    Label="Fecha operación"
                    LabelPosition="Above">
                    <A2UTILGEN:A2DatePicker
                        x:Name="dtmFechaOperacion"
                        HorizontalAlignment="Left"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                        SelectedDate="{Binding FechaOperacion, Mode=TwoWay}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="5"
                    Label="Fecha cumplimiento"
                    LabelPosition="Above">
                    <A2UTILGEN:A2DatePicker
                        x:Name="dtmFechaCumplimiento"
                        HorizontalAlignment="Left"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                        SelectedDate="{Binding FechaCumplimiento, Mode=TwoWay}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="6"
                    Label="Tipo cumplimiento"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboTipoCumplimiento"
                        DisplayMemberPath="Descripcion"
                        IsEnabled="False"
                        ItemsSource="{Binding DiccionarioCombos[NEGOCIOS_TIPOCUMP], Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding TipoCumplimiento, Mode=TwoWay}"
                        SelectedValuePath="Codigo" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="7"
                    Label="Tipo"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboTipo"
                        DisplayMemberPath="Descripcion"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARTIPO], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding DiccionarioCombos[NEGOCIOS_TIPO], Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding Tipo, Mode=TwoWay}"
                        SelectedValuePath="Codigo" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="1"
                    Grid.Column="8"
                    Label="Exponencial base 365"
                    LabelPosition="Above"
                    Visibility="{Binding MostrarTipoBanRep, Source={StaticResource VMOperaciones}}">
                    <CheckBox IsChecked="{Binding TipoBanRep, Mode=TwoWay}" IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="0"
                    Label="Clasificación inversión"
                    LabelPosition="Above">

                    <ComboBox
                        x:Name="cboClasificacionInversion"
                        DisplayMemberPath="Descripcion"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[CLASIFICACIONINVERSION], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding DiccionarioCombos[NEGOCIOS_CLASEINVERSION], Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding ClasificacionInversion, Mode=TwoWay}"
                        SelectedValuePath="Codigo" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="1"
                    Label="Mercado"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboMercado"
                        DisplayMemberPath="Descripcion"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[MERCADO], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding DiccionarioCombos[NEGOCIOS_MERCADO], Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding Mercado, Mode=TwoWay}"
                        SelectedValuePath="Codigo" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="2"
                    Label="Tipo repo"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboTipoRepo"
                        DisplayMemberPath="Descripcion"
                        IsEnabled="{Binding DiccionarioHabilitarCampos[TIPOREPO], Source={StaticResource VMOperaciones}}"
                        ItemsSource="{Binding DiccionarioCombos[NEGOCIOS_TIPOREPO], Source={StaticResource VMOperaciones}}"
                        SelectedValue="{Binding TipoRepo, Mode=TwoWay}"
                        SelectedValuePath="Codigo" />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="3"
                    Label="País cumplimiento"
                    LabelPosition="Above">
                    <StackPanel Orientation="Horizontal">
                        <StackPanel Background="{Binding FondoTextoBuscadores, Source={StaticResource VMOperaciones}}">
                            <Border BorderBrush="Gray" BorderThickness="0.5">
                                <TextBlock
                                    x:Name="txtPais"
                                    Height="19"
                                    MinWidth="120"
                                    Margin="2,0,0,0"
                                    VerticalAlignment="Center"
                                    MouseLeftButtonDown="txtPais_MouseLeftButtonDown"
                                    Style="{StaticResource ControlTextBlockNormal}"
                                    Text="{Binding NombrePaisNegociacion, Mode=TwoWay}" />
                            </Border>
                        </StackPanel>
                        <Controles:BuscadorGenericoListaButon
                            x:Name="ctlBuscadorPais"
                            Margin="2,0,0,0"
                            Agrupamiento=""
                            CampoBusqueda="Paises"
                            EstadoItem="A"
                            Etiqueta="Pais"
                            IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARPAIS], Source={StaticResource VMOperaciones}}"
                            TipoItem="Paises"
                            finalizoBusqueda="BuscadorGenericoListaButon_finalizoBusqueda" />
                    </StackPanel>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="4"
                    Grid.ColumnSpan="2"
                    Margin="2"
                    HorizontalAlignment="Stretch"
                    Label="Contraparte"
                    Visibility="{Binding MostrarContraparte, Source={StaticResource VMOperaciones}}">
                    <StackPanel Orientation="Horizontal">
                        <StackPanel Background="{Binding FondoTextoBuscadores, Source={StaticResource VMOperaciones}}">
                            <Border
                                Height="20"
                                BorderBrush="Gray"
                                BorderThickness="0.5">
                                <TextBlock
                                    x:Name="txtContraparte"
                                    Height="19"
                                    MinWidth="260"
                                    Margin="2,0,0,0"
                                    VerticalAlignment="Center"
                                    MouseLeftButtonDown="txtContraparte_MouseLeftButtonDown"
                                    Style="{StaticResource ControlTextBlockNormal}"
                                    Text="{Binding NombreContraparte, Mode=TwoWay}" />
                            </Border>
                        </StackPanel>
                        <Controles:BuscadorGenericoListaButon
                            x:Name="ctlBuscadorContraparte"
                            Margin="2,0,0,0"
                            Agrupamiento=""
                            CampoBusqueda="Contraparte"
                            EstadoItem="A"
                            Etiqueta="Contraparte"
                            IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                            TipoItem="Entidades"
                            finalizoBusqueda="BuscadorGenericoListaButon_finalizoBusqueda" />
                    </StackPanel>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="4"
                    Grid.ColumnSpan="2"
                    Label="Contraparte"
                    LabelPosition="Above"
                    Visibility="{Binding MostrarContraparteOTC, Source={StaticResource VMOperaciones}}">

                    <StackPanel Orientation="Horizontal">
                        <StackPanel Background="{Binding FondoTextoBuscadores, Source={StaticResource VMOperaciones}}">
                            <Border
                                Height="20"
                                BorderBrush="Gray"
                                BorderThickness="0.5">
                                <TextBlock
                                    x:Name="txtContrapartePP"
                                    Height="19"
                                    MinWidth="260"
                                    VerticalAlignment="Center"
                                    MouseLeftButtonDown="txtContraparte_MouseLeftButtonDown"
                                    Style="{StaticResource ControlTextBlockNormal}"
                                    Text="{Binding NombreContraparte, Mode=TwoWay}" />
                            </Border>
                        </StackPanel>
                        <Controles:BuscadorGenericoListaButon
                            x:Name="ctlBuscadorContrapartePP"
                            Margin="2,0,0,0"
                            Agrupamiento=""
                            CampoBusqueda="Contraparte"
                            EstadoItem="A"
                            Etiqueta="Contraparte"
                            IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                            TipoItem="Entidades"
                            finalizoBusqueda="BuscadorGenericoListaButon_finalizoBusqueda" />
                    </StackPanel>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="6"
                    Label="Código OYD"
                    LabelPosition="Above"
                    Visibility="{Binding MostrarContraparteOTC, Source={StaticResource VMOperaciones}}">
                    <Grid>
                        <TextBox
                            MinWidth="120"
                            IsEnabled="False"
                            Text="{Binding IDComitenteOtro, Mode=TwoWay}"
                            Visibility="{Binding Editando, Source={StaticResource VMOperaciones}, Converter={StaticResource InVisibilityConverter}}" />
                        <ComboBox
                            MinWidth="120"
                            DisplayMemberPath="Descripcion"
                            IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                            ItemsSource="{Binding ListaComitentesContraparte, Source={StaticResource VMOperaciones}, Mode=OneWay}"
                            SelectedValue="{Binding IDComitenteOtro, Mode=TwoWay}"
                            SelectedValuePath="ID"
                            Visibility="{Binding Editando, Source={StaticResource VMOperaciones}, Converter={StaticResource VisibilityConverter}}" />
                    </Grid>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="7"
                    Label="Cuenta depósito contraparte"
                    LabelPosition="Above"
                    Visibility="{Binding MostrarContraparte, Source={StaticResource VMOperaciones}}">
                    <Grid>
                        <TextBox
                            IsEnabled="False"
                            Text="{Binding DescripcionCuentaDepositoContraparte, Mode=TwoWay}"
                            Visibility="{Binding Editando, Source={StaticResource VMOperaciones}, Converter={StaticResource InVisibilityConverter}}" />
                        <ComboBox
                            DisplayMemberPath="DescripcionCuenta"
                            IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARCONTRAPARTE], Source={StaticResource VMOperaciones}}"
                            ItemsSource="{Binding ListaCuentasDepositoContraparte, Source={StaticResource VMOperaciones}, Mode=OneWay}"
                            SelectedItem="{Binding CtaDepositoSeleccionadaContraparte, Source={StaticResource VMOperaciones}, Mode=TwoWay}"
                            Visibility="{Binding Editando, Source={StaticResource VMOperaciones}, Converter={StaticResource VisibilityConverter}}" />
                    </Grid>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField
                    Grid.Row="2"
                    Grid.Column="7"
                    Label="Cuenta depósito contraparte"
                    LabelPosition="Above"
                    Visibility="{Binding MostrarContraparteOTC, Source={StaticResource VMOperaciones}}">
                    <StackPanel>
                        <TextBox
                            IsEnabled="False"
                            Text="{Binding DescripcionCuentaDepositoContraparte, Mode=TwoWay}"
                            Visibility="{Binding Editando, Source={StaticResource VMOperaciones}, Converter={StaticResource InVisibilityConverter}}" />
                        <ComboBox
                            Height="20"
                            ItemsSource="{Binding ListaCuentasDepositoContraparteOTC, Source={StaticResource VMOperaciones}, Mode=OneWay}"
                            SelectedItem="{Binding CtaDepositoSeleccionadaContraparteOTC, Source={StaticResource VMOperaciones}, Mode=TwoWay}"
                            SelectedValuePath="NroCuentaDeposito"
                            Visibility="{Binding Editando, Source={StaticResource VMOperaciones}, Converter={StaticResource VisibilityConverter}}">
                            <ComboBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel
                                        Height="Auto"
                                        MaxWidth="320"
                                        Orientation="Vertical"
                                        ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                        ScrollViewer.VerticalScrollBarVisibility="Auto" />
                                </ItemsPanelTemplate>
                            </ComboBox.ItemsPanel>

                            <ComboBox.ItemTemplate>
                                <DataTemplate>
                                    <StackPanel
                                        Height="Auto"
                                        Margin="0"
                                        HorizontalAlignment="Left">
                                        <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=DescripcionCta}" />
                                        <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NombreBeneficiario1}" />
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=TipoIdBeneficiario1}" />
                                            <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NroDocBeneficiario1, StringFormat='nro. \{0}'}" />
                                        </StackPanel>

                                        <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NombreBeneficiario2}" />
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=TipoIdBeneficiario2}" />
                                            <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NroDocBeneficiario2, StringFormat='nro. \{0}'}" />
                                        </StackPanel>

                                        <Line
                                            Height="1"
                                            Margin="0"
                                            Stretch="Fill"
                                            Stroke="Silver"
                                            X1="0"
                                            X2="100"
                                            Y1="20"
                                            Y2="20" />
                                    </StackPanel>
                                </DataTemplate>
                            </ComboBox.ItemTemplate>

                            <ToolTipService.ToolTip>
                                <ToolTip>
                                    <StackPanel DataContext="{Binding CtaDepositoSeleccionadaContraparteOTC, Source={StaticResource VMOperaciones}}" Orientation="Vertical">

                                        <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=DescripcionCta}" />
                                        <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NombreBeneficiario1}" />

                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                                <RowDefinition />
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="20" />
                                                <ColumnDefinition />
                                            </Grid.ColumnDefinitions>

                                            <TextBlock
                                                Grid.Row="0"
                                                Grid.ColumnSpan="2"
                                                Style="{StaticResource ControlTextBlockNormalInfo}"
                                                Text="Beneficiarios" />

                                            <TextBlock
                                                Grid.Row="1"
                                                Grid.Column="1"
                                                Style="{StaticResource ControlTextBlockNormalInfo}"
                                                Text="{Binding Path=NombreBeneficiario1}" />
                                            <StackPanel
                                                Grid.Row="2"
                                                Grid.Column="1"
                                                Orientation="Horizontal">
                                                <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=TipoIdBeneficiario1}" />
                                                <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text=" " />
                                                <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NroDocBeneficiario1}" />
                                            </StackPanel>

                                            <TextBlock
                                                Grid.Row="3"
                                                Grid.Column="1"
                                                Style="{StaticResource ControlTextBlockNormalInfo}"
                                                Text="{Binding Path=NombreBeneficiario2}" />
                                            <StackPanel
                                                Grid.Row="4"
                                                Grid.Column="1"
                                                Orientation="Horizontal">
                                                <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=TipoIdBeneficiario2}" />
                                                <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text=" " />
                                                <TextBlock Style="{StaticResource ControlTextBlockNormalInfo}" Text="{Binding Path=NroDocBeneficiario2}" />
                                            </StackPanel>
                                        </Grid>
                                    </StackPanel>
                                </ToolTip>
                            </ToolTipService.ToolTip>
                        </ComboBox>
                    </StackPanel>
                </Telerik:DataFormDataField>
                <!--<StackPanel Grid.Row="2" Grid.Column="7" Grid.ColumnSpan="2"
                            Orientation="Vertical"
                            HorizontalAlignment="Stretch"
                            Margin="2">
                    <Label Content="Pagador"
                               Style="{StaticResource EstiloLabel}"/>
                    <StackPanel Orientation="Horizontal">
                        <StackPanel Background="{Binding FondoTextoBuscadores, Source={StaticResource VMOperaciones}}">
                            <TextBlock x:Name="txtPagador"
                                 Text="{Binding NombrePagador, Mode=TwoWay}"
                                       MouseLeftButtonDown="txtPagador_MouseLeftButtonDown"
                                       MinWidth="120"
                                       />
                        </StackPanel>
                        <Controles:BuscadorGenericoListaButon CampoBusqueda="Pagador"
                                                              Etiqueta="Pagador"
                                                              TipoItem="Entidades"
                                                              EstadoItem="A"
                                                              Agrupamiento=""
                                                              finalizoBusqueda="BuscadorGenericoListaButon_finalizoBusqueda"
                                                              IsEnabled="{Binding DiccionarioHabilitarCampos[HABILITARNEGOCIO], Source={StaticResource VMOperaciones}}"
                                                              x:Name="ctlBuscadorPagador"/>
                    </StackPanel>
                </StackPanel>-->
            </Grid>
        </Border>

        <Grid
            Grid.Row="2"
            Grid.Column="0"
            Margin="0,2,0,2"
            HorizontalAlignment="Stretch">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <Border
                Grid.Column="0"
