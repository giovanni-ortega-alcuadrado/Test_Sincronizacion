<UserControl
    x:Class="CompaniasView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:A2UTILGEN="clr-namespace:A2Utilidades;assembly=A2.Controles.Utilidades.WPF"
    xmlns:A2Utils="clr-namespace:A2ControlMenu;assembly=A2ControlMenu.WPF"
    xmlns:Controles="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:OyD="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:Telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:my="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:my1="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:src="clr-namespace:A2CFCalculosFinancieros"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    MinWidth="1000"
    MaxWidth="1310"
    HorizontalAlignment="Stretch"
    mc:Ignorable="d">

    <UserControl.Resources>
        <src:DisplayNameConverter x:Key="DisplayConverter" />
        <src:VisibilityConverter x:Key="VisibilityConverter" />
        <!--<src:A2UtilsViewModel x:Key="A2VM"/>-->
        <src:BooleanConverter x:Key="BooleanConverter" />
        <src:IntegerConverter x:Key="IntegerConverter" />
        <src:BooleanoConverterRadio x:Key="BooleanoConverterRadio" />
        <src:BooleanoConverterContrario x:Key="BooleanoConverterContrario" />
        <src:StringConverter x:Key="StringConverter" />
        <src:IntConverterBoolean x:Key="IntConverterBoolean" />
    </UserControl.Resources>

    <!--
        Descripción:    Diseño para la pantalla Compañías (Cálculos Financieros)
        Responsable:    Javier Pardo (Alcuadrado S.A.)
        Fecha:          Julio 29/2015
    -->

    <Telerik:RadBusyIndicator
        x:Name="myBusyIndicator"
        BusyContent="Cargando..."
        DisplayAfter="0"
        IsBusy="{Binding IsBusy}">
        <Grid x:Name="LayoutRoot" ShowGridLines="False">
            <Grid.RowDefinitions>
                <!--  Fila del menu  -->
                <RowDefinition Height="Auto" MinHeight="20" />
                <!--  Fila de la forma/grid  -->
                <RowDefinition Height="Auto" MinHeight="43" />
            </Grid.RowDefinitions>

            <StackPanel Grid.Row="0">
                <A2Utils:ControlMenuA2
                    x:Name="cm"
                    DescripcionFiltro="{Binding DescripcionFiltroVM, Mode=TwoWay}"
                    DicBotonesMenu="{Binding DicBotonesMenuVM, Mode=TwoWay}"
                    EstadoMenu="{Binding EstadoMenuVM, Mode=TwoWay}"
                    Filtro="{Binding FiltroVM, Mode=TwoWay}"
                    HayDataForm="False"
                    IsItemValid="True"
                    ItemGridHyperlink="{Binding ItemSeleccionadoHyperlink, Mode=TwoWay}"
                    Mensaje="{Binding MensajeMVVM, Mode=TwoWay}"
                    MensajeError="{Binding ErrorForma}"
                    Modo="{Binding ModoMVVM, Mode=TwoWay}"
                    ObjetoSeleccionado="{Binding EncabezadoSeleccionado}"
                    PreguntarConfirmacion="False"
                    TextoFiltroTooltip="Tipo documento, número documento, nombre o tipo compañía"
                    TituloVista="Compañías"
                    TituloVistaAdicional="registros"
                    TituloVista_CambioSelected="{Binding EncabezadoSeleccionado.HasChanges}"
                    TituloVista_CantidadRegistros="{Binding ListaEncabezado.Count}"
                    VisibilidadRefrescarCombos="True" />
            </StackPanel>

            <Border Grid.Row="1" Style="{StaticResource Contenedor_Estilo}">
                <Grid x:Name="grdGridForma">
                    <Border>
                        <!--  ===========================  -->
                        <!--  Grid de navegación paginada  -->
                        <!--  ===========================  -->
                        <StackPanel x:Name="Buscar" VerticalAlignment="Top">
                            <Border Style="{StaticResource Contenedor_Paginador}">
                                <A2UTILGEN:A2DataPager
                                    x:Name="datapager1"
                                    PageSize="15"
                                    Source="{Binding Items, ElementName=dg}" />
                            </Border>
                            <A2UTILGEN:A2GridView
                                x:Name="dg"
                                Height="360"
                                VerticalAlignment="Top"
                                AutoGenerateColumns="False"
                                FrozenColumnCount="1"
                                IsReadOnly="True"
                                ItemsSource="{Binding ListaEncabezadoPaginada}"
                                ScrollViewer.VerticalScrollBarVisibility="Visible"
                                SelectedItem="{Binding EncabezadoSeleccionado, Mode=TwoWay}"
                                ValidatesOnDataErrors="None">
                                <A2UTILGEN:A2GridView.Columns>

                                    <Telerik:GridViewDataColumn Header="Tipo compañía" SortMemberPath="Tipo compañía">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                    VerticalAlignment="Center"
                                                    Command="{Binding DataContext.Hyperlink_CambiarAForma, RelativeSource={RelativeSource AncestorType=A2UTILGEN:A2GridView}}"
                                                    CommandParameter="{Binding}"
                                                    Content="{Binding strTipoCompaniaDescripcion, Mode=TwoWay}"
                                                    Style="{StaticResource HyperlinkLikeButton}" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>


                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding intIDCompania}" Header="Código" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strNombre}" Header="Nombre" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strTipoDocumentoDescripcion}" Header="Tipo documento" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strNumeroDocumento}" Header="Nro. documento" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding dtmFechaVencimiento}" Header="Fecha vencimiento" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strEstadoCompania}" Header="Estado" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strTipoPlazoDescripcion}" Header="Tipo plazo" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strParticipacionDescripcion}" Header="Tipo participación" />

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding strUltimaFechaCierre}" Header="Fecha último cierre" />

                                </A2UTILGEN:A2GridView.Columns>
                            </A2UTILGEN:A2GridView>
                        </StackPanel>
                    </Border>

                    <Border VerticalAlignment="Top">
                        <Grid>
                            <!--  ====================  -->
                            <!--  Dataform de búsqueda  -->
                            <!--  ====================  -->
                            <A2UTILGEN:A2DataForm
                                x:Name="dfBuscar"
                                VerticalAlignment="Top"
                                AutoGenerateFields="False"
                                BorderThickness="0"
                                CommandButtonsVisibility="None"
                                CurrentItem="{Binding cb}"
                                Visibility="{Binding visBuscando}">
                                <A2UTILGEN:A2DataForm.EditTemplate>
                                    <DataTemplate>
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                                <RowDefinition Height="Auto" />
                                            </Grid.RowDefinitions>
                                            <!--  Número documento  -->
                                            <Telerik:DataFormDataField
                                                Grid.Row="0"
                                                Label="Número documento"
                                                LabelPosition="Above">
                                                <TextBox
                                                    AcceptsReturn="True"
                                                    GotFocus="seleccionarFocoControl"
                                                    MaxLength="15"
                                                    Text="{Binding strNumeroDocumento, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    TextAlignment="Left"
                                                    TextWrapping="Wrap"
                                                    ToolTipService.ToolTip="Número documento" />
                                            </Telerik:DataFormDataField>

                                            <!--  Tipo documento  -->
                                            <Telerik:DataFormDataField
                                                Grid.Row="1"
                                                Label="Tipo documento"
                                                LabelPosition="Above">
                                                <ComboBox
                                                    DisplayMemberPath="Descripcion"
                                                    GotFocus="seleccionarFocoControl"
                                                    ItemsSource="{Binding DiccionarioCombos[TIPOID], Source={StaticResource A2VM}}"
                                                    SelectedValue="{Binding strTipoDocumento, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    SelectedValuePath="ID"
                                                    ToolTipService.ToolTip="Tipo Documento" />
                                            </Telerik:DataFormDataField>

                                            <!--  Tipo compañía  -->
                                            <Telerik:DataFormDataField
                                                Grid.Row="2"
                                                Label="Tipo compañía"
                                                LabelPosition="Above">
                                                <ComboBox
                                                    DisplayMemberPath="Descripcion"
                                                    GotFocus="seleccionarFocoControl"
                                                    ItemsSource="{Binding DataContext.ListaTipoCompaniaBusqueda, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    SelectedValue="{Binding strTipoCompania, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    SelectedValuePath="ID"
                                                    ToolTipService.ToolTip="Tipo compañía" />
                                            </Telerik:DataFormDataField>
                                            <!--  Nombre  -->
                                            <Telerik:DataFormDataField
                                                Grid.Row="3"
                                                Label="Nombre"
                                                LabelPosition="Above">
                                                <TextBox
                                                    GotFocus="seleccionarFocoControl"
                                                    MaxLength="50"
                                                    Text="{Binding strNombre, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    TextAlignment="Left"
                                                    ToolTipService.ToolTip="Nombre" />
                                            </Telerik:DataFormDataField>
                                            <!--  Tipo Plazo  -->
                                            <Telerik:DataFormDataField Grid.Row="4" Label="Tipo Plazo">
                                                <ComboBox
                                                    DisplayMemberPath="Descripcion"
                                                    GotFocus="seleccionarFocoControl"
                                                    ItemsSource="{Binding DataContext.ListaTipoPlazoBusqueda, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    SelectedValue="{Binding strTipoPlazo, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    SelectedValuePath="ID"
                                                    ToolTipService.ToolTip="Tipo plazo" />
                                            </Telerik:DataFormDataField>
                                            <!--  Tipo Participacion  -->
                                            <Telerik:DataFormDataField Grid.Row="5" Label="Tipo Participación">
                                                <ComboBox
                                                    DisplayMemberPath="Descripcion"
                                                    GotFocus="seleccionarFocoControl"
                                                    ItemsSource="{Binding DiccionarioCombos[TIPOPARTICIPACION], Source={StaticResource A2VM}}"
                                                    SelectedValue="{Binding strParticipacion, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    SelectedValuePath="ID"
                                                    ToolTipService.ToolTip="Tipo participación" />
                                            </Telerik:DataFormDataField>

                                        </Grid>
                                    </DataTemplate>
                                </A2UTILGEN:A2DataForm.EditTemplate>
                            </A2UTILGEN:A2DataForm>

                            <StackPanel x:Name="Editar" Visibility="{Binding visNavegando}">
                                <!--  ============================  -->
                                <!--  Dataform edición de registro  -->
                                <!--  ============================  -->
                                <Border>
                                    <Grid
                                        x:Name="gridEd"
                                        VerticalAlignment="Top"
                                        DataContext="{Binding EncabezadoSeleccionado}">

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                            <RowDefinition Height="Auto" />
                                        </Grid.RowDefinitions>

                                        <!--  Tipo Identificación  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            Label="Tipo identificación">
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="False"
                                                ItemsSource="{Binding DiccionarioCombos[TIPOID], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding strTipoDocumento, StringFormat='#', Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                SelectedValuePath="ID"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Tipo Identificacion" />
                                        </Telerik:DataFormDataField>
                                        <!--  Tipo Identificación  -->

                                        <!--  Número identificación  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="0"
                                            Grid.Column="1"
                                            Label="Número identificación">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>

                                                <TextBox
                                                    x:Name="txtPortafolio"
                                                    GotFocus="seleccionarFocoControl"
                                                    IsEnabled="{Binding DataContext.HabilitarCamposEdicion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    MaxLength="15"
                                                    TabIndex="-1"
                                                    Text="{Binding strNumeroDocumento, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    TextAlignment="Left"
                                                    ToolTipService.ToolTip="{Binding DataContext.NOMBRE_ETIQUETA_COMITENTE, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                                                <my1:BuscadorClienteListaButon
                                                    Grid.Column="1"
                                                    Margin="2,0,0,0"
                                                    Agrupamiento="todoslosreceptores"
                                                    CampoBusqueda="IdComitenteBuscar"
                                                    EstadoComitente="A"
                                                    IsEnabled="{Binding DataContext.HabilitarCamposEdicion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    TabIndex="-1"
                                                    TipoVinculacion="C"
                                                    finalizoBusqueda="ctrlCliente_comitente_Dataform_Edicion" />
                                                <Button
                                                    x:Name="btnLimpiarCliente"
                                                    Grid.Column="2"
                                                    Margin="2,0,0,0"
                                                    Click="btnLimpiarCliente_Dataform_Edicion_Click"
                                                    IsEnabled="{Binding DataContext.HabilitarCamposEdicion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    Style="{StaticResource btnLimpiar}"
                                                    TabIndex="-1"
                                                    ToolTipService.ToolTip="Limpiar cliente" />
                                            </Grid>
                                        </Telerik:DataFormDataField>
                                        <!--  Número identificación  -->

                                        <Grid
                                            Grid.Row="0"
                                            Grid.Column="2"
                                            Grid.ColumnSpan="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="80*" />
                                                <ColumnDefinition Width="20*" />
                                            </Grid.ColumnDefinitions>

                                            <!--  Nombre  -->
                                            <Telerik:DataFormDataField
                                                x:Name="SpNombre"
                                                Grid.Column="0"
                                                Label="Nombre">
                                                <TextBox
                                                    IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    IsReadOnly="True"
                                                    MaxLength="50"
                                                    TabIndex="-1"
                                                    Text="{Binding strNombre, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    TextAlignment="Left"
                                                    ToolTipService.ToolTip="Nombre" />
                                            </Telerik:DataFormDataField>
                                            <!--  Nombre  -->

                                            <!--  Código  -->
                                            <Telerik:DataFormDataField Grid.Column="1" Label="Código">
                                                <TextBox
                                                    IsEnabled="False"
                                                    MaxLength="50"
                                                    TabIndex="-1"
                                                    Text="{Binding intIDCompania, Mode=OneWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    TextAlignment="Left"
                                                    ToolTipService.ToolTip="Código compañía" />
                                            </Telerik:DataFormDataField>
                                            <!--  Código  -->
                                        </Grid>

                                        <!--  Nombre Corto  -->
                                        <Telerik:DataFormDataField
                                            x:Name="SpAlias"
                                            Grid.Row="0"
                                            Grid.Column="5"
                                            Label="Nombre corto">
                                            <TextBox
                                                x:Name="TxtNombreCorto"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                MaxLength="50"
                                                TabIndex="-1"
                                                Text="{Binding strNombreCorto, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                TextAlignment="Left"
                                                TextChanged="TxtNombreCorto_TextChanged"
                                                ToolTipService.ToolTip="Nombre" />
                                        </Telerik:DataFormDataField>
                                        <!--  Nombre Corto  -->

                                        <!--  Estado  -->
                                        <Telerik:DataFormDataField
                                            x:Name="Estado"
                                            Grid.Row="0"
                                            Grid.Column="6"
                                            IsEnabled="{Binding DataContext.HabilitarEstado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                            Label="Estado"
                                            LabelPosition="Above">
                                            <ComboBox
                                                x:Name="cbEstado"
                                                SelectedIndex="{Binding logActiva, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, Converter={StaticResource IntConverterBoolean}}"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Estado">
                                                <ComboBoxItem>Inactivo</ComboBoxItem>
                                                <ComboBoxItem>Activo</ComboBoxItem>
                                            </ComboBox>
                                        </Telerik:DataFormDataField>
                                        <!--  Estado  -->

                                        <!--  Tipo compañía  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="0"
                                            Grid.Column="7"
                                            IsEnabled="{Binding DataContext.HabilitarTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                            Label="Tipo compañía"
                                            LabelPosition="Above">
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                ItemsSource="{Binding DataContext.ListaTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                SelectedValue="{Binding strTipoCompania, StringFormat='#', Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                SelectedValuePath="ID"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Tipo compañía" />
                                        </Telerik:DataFormDataField>

                                        <!--  Compañía Contable  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="0"
                                            Grid.Column="8"
                                            Label="Compañía contable"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <TextBox
                                                x:Name="txtCompania"
                                                GotFocus="seleccionarFocoControl"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                MaxLength="20"
                                                TabIndex="-1"
                                                Text="{Binding strCompaniaContable, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                TextAlignment="Left"
                                                ToolTipService.ToolTip="Compañía contable" />
                                        </Telerik:DataFormDataField>
                                        <!--  Compañía Contable  -->

                                        <!--  Tipo entidad Super  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="0"
                                            Label="Tipo entidad Superintendencia"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <TextBox
                                                x:Name="txtTipoEntidadSuper"
                                                GotFocus="seleccionarFocoControl"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                MaxLength="10"
                                                TabIndex="-1"
                                                Text="{Binding strTipoEntidadSuper, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                TextAlignment="Left"
                                                ToolTipService.ToolTip="Tipo Entidad Superintendencia Financiera" />
                                        </Telerik:DataFormDataField>
                                        <!--  Tipo entidad Super  -->

                                        <!--  Código Super  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Label="Código Superintendencia"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <TextBox
                                                x:Name="txtCodigoSuper"
                                                GotFocus="seleccionarFocoControl"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                MaxLength="10"
                                                TabIndex="-1"
                                                Text="{Binding strCodigoSuper, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                TextAlignment="Left"
                                                ToolTipService.ToolTip="Código Superintendencia Financiera" />

                                        </Telerik:DataFormDataField>
                                        <!--  Código Super  -->

                                        <!--  Proveedorejemplo precios  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="2"
                                            Grid.ColumnSpan="2"
                                            Label="Proveedor precios"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                ItemsSource="{Binding DiccionarioCombos[CF_ProveedorPrecios], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding strProveedorPrecios, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                SelectedValuePath="ID"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Proveedor precios" />
                                        </Telerik:DataFormDataField>
                                        <!--  Proveedorejemplo precios  -->

                                        <!--  Contabilización Dividendos  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="4"
                                            Grid.ColumnSpan="2"
                                            Label="Contabilización dividendos"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                ItemsSource="{Binding DiccionarioCombos[CONTABDIVIDENDO], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding strContabDividendos, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                SelectedValuePath="ID"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Contabilización de pagos de dividendos" />
                                        </Telerik:DataFormDataField>
                                        <!--  Contabilización Dividendos  -->

                                        <!--  Configuración contable  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="6"
                                            Label="Configuración contable"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                ItemsSource="{Binding DiccionarioCombos[CFCONTABILIZACIONDIVIDENDOS], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding intIdDinamicaContableConfig, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True, Converter={StaticResource StringConverter}}"
                                                SelectedValuePath="ID"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Configuración contable" />
                                        </Telerik:DataFormDataField>
                                        <!--  Configuración contable  -->

                                        <!--  Fecha Apertura  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="7"
                                            Label="Fecha apertura"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <A2UTILGEN:A2DatePicker
                                                IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                SelectedDate="{Binding dtmApertura, StringFormat='dd/MMM/yyyy', Mode=TwoWay}"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Fecha apertura" />
                                        </Telerik:DataFormDataField>
                                        <!--  Fecha Apertura  -->

                                        <!--  Fecha Ultimo Cierre  -->
                                        <Telerik:DataFormDataField
                                            Grid.Row="1"
                                            Grid.Column="8"
                                            Label="Fecha último cierre"
                                            Visibility="{Binding DataContext.MostrarControlesTipoCompania, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <A2UTILGEN:A2DatePicker
                                                IsEnabled="False"
                                                SelectedDate="{Binding dtmUltimoCierre, StringFormat='dd/MMM/yyyy', Mode=TwoWay}"
                                                TabIndex="-1"
                                                ToolTipService.ToolTip="Fecha último cierre" />
                                        </Telerik:DataFormDataField>
                                        <!--  Fecha Ultimo Cierre  -->

