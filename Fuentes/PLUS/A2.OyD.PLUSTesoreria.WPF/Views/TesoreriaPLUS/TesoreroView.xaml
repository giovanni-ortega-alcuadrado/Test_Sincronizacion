<UserControl
    x:Class="TesoreroView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:A2UTILGEN="clr-namespace:A2Utilidades;assembly=A2.Controles.Utilidades.WPF"
    xmlns:Controles="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:ControlesPLUS="clr-namespace:A2OYDPLUSUtilidades;assembly=A2.OyD.PLUSUtilidades.WPF"
    xmlns:Telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:src="clr-namespace:A2OYDPLUSTesoreria"
    mc:Ignorable="d">
    <UserControl.Resources>
        <src:DisplayNameConverter x:Key="DisplayConverter" />
        <src:VisibilityConverter x:Key="VisibilityConverter" />
        <src:clsVisibilityConverter x:Key="clsVisibilityConverter" />
        <src:InVisibilityConverter x:Key="InVisibilityConverter" />
        <src:ColorFilaConverter x:Key="ColorFilaConverter" />
        <src:BooleanVisibilityConverter x:Key="BooleanVisibilityConverter" />
        <src:InBooleanConverter x:Key="InBooleanConverter" />
    </UserControl.Resources>

    <Telerik:RadBusyIndicator
        x:Name="myBusyIndicator"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        BusyContent="Cargando..."
        DisplayAfter="0"
        IsBusy="{Binding IsBusy}">

        <Border
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            Style="{StaticResource Contenedor_Estilo}">
            <Grid
                x:Name="LayoutRoot"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Stretch">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <Telerik:DataFormDataField
                    Grid.Row="0"
                    Grid.Column="0"
                    VerticalAlignment="Top"
                    Label="Tipo negocio"
                    LabelPosition="Above">
                    <ComboBox
                        x:Name="cboTiponegocio"
                        Width="130"
                        VerticalAlignment="Top"
                        DisplayMemberPath="Descripcion"
                        ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[ORDENESTESORERIA], RelativeSource={RelativeSource AncestorType=UserControl}}"
                        SelectedValue="{Binding strTipoNegocio, Mode=TwoWay}"
                        SelectedValuePath="Retorno" />
                </Telerik:DataFormDataField>
                <Grid
                    Grid.Column="1"
                    HorizontalAlignment="Stretch"
                    Visibility="{Binding DataContext.HabilitarFiltrosOrdenesGiro, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource VisibilityConverter}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>

                        <Telerik:DataFormDataField
                            Grid.Row="0"
                            Grid.Column="0"
                            Label="Sucursal"
                            LabelPosition="Above">
                            <ComboBox
                                DisplayMemberPath="Descripcion"
                                ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[SUCURSAL_PLUS], RelativeSource={RelativeSource AncestorType=UserControl}}"
                                SelectedValue="{Binding strSUCURSAL_PLUS, Mode=TwoWay}"
                                SelectedValuePath="Retorno" />
                        </Telerik:DataFormDataField>
                        <Telerik:DataFormDataField
                            Grid.Row="0"
                            Grid.Column="1"
                            Label="Receptor"
                            LabelPosition="Above">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBox
                                    Grid.Column="0"
                                    Background="{Binding DataContext.FondoTextoBuscadoresHabilitado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                    MouseLeftButtonDown="txtReceptor1_MouseLeftButtonDown"
                                    Text="{Binding DataContext.NombreReceptor, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                <Controles:BuscadorGenericoListaButon
                                    x:Name="ctlBuscadorReceptor1"
                                    Grid.Column="1"
                                    Margin="2,0,0,0"
                                    HorizontalAlignment="Left"
                                    CampoBusqueda="receptores"
                                    EstadoItem="A"
                                    TipoItem="receptoresusuario"
                                    finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                                <Button
                                    Grid.Column="2"
                                    Margin="2,0,0,0"
                                    VerticalAlignment="Bottom"
                                    Click="btnLimpiarReceptor_Click_1"
                                    Style="{StaticResource btnLimpiar}"
                                    ToolTipService.ToolTip="Limpiar" />
                            </Grid>
                        </Telerik:DataFormDataField>
                        <Telerik:DataFormDataField
                            Grid.Row="0"
                            Grid.Column="2"
                            IsEnabled="{Binding HabilitarFormaPago}"
                            Label="Forma Pago"
                            LabelPosition="Above">
                            <ComboBox
                                DisplayMemberPath="Descripcion"
                                ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[TIPOPAGOPLUS], RelativeSource={RelativeSource AncestorType=UserControl}}"
                                SelectedValue="{Binding strTipoPagoPlus, Mode=TwoWay}"
                                SelectedValuePath="Retorno" />
                        </Telerik:DataFormDataField>
                        <Telerik:DataFormDataField
                            Grid.Row="0"
                            Grid.Column="3"
                            Label="Estado"
                            LabelPosition="Above">
                            <ComboBox
                                DisplayMemberPath="Descripcion"
                                ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[ESTADOS_TESORERO], RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                SelectedValue="{Binding strEstado, Mode=TwoWay}"
                                SelectedValuePath="Retorno" />
                        </Telerik:DataFormDataField>
                        <Telerik:DataFormDataField
                            Grid.Row="0"
                            Grid.Column="4"
                            Label="Fecha orden"
                            LabelPosition="Above">
                            <A2UTILGEN:A2DatePicker SelectedDate="{Binding Fecha, Mode=TwoWay}" />
                        </Telerik:DataFormDataField>
                        <Telerik:DataFormDataField
                            Grid.Row="0"
                            Grid.Column="5"
                            Label="Fondos"
                            LabelPosition="Above"
                            Visibility="{Binding DataContext.HabilitarFiltrosCarteraColectiva, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource VisibilityConverter}}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBox
                                    Width="200"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Center"
                                    MouseLeftButtonDown="txtCartera1_MouseLeftButtonDown"
                                    Text="{Binding DataContext.DescripcionCarteraColectiva, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                <Controles:BuscadorGenericoListaButon
                                    x:Name="ctlBuscadorCartera1"
                                    Grid.Column="1"
                                    Margin="2,0,0,0"
                                    CampoBusqueda="carterascolectivas"
                                    Condicion1="{Binding DataContext.FechaConvertida, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                    Condicion2="{Binding DataContext.strEstado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                    EstadoItem="A"
                                    TipoItem="FONDOSTESORERO"
                                    finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                                <Button
                                    Grid.Column="2"
                                    Margin="2,0,0,0"
                                    VerticalAlignment="Bottom"
                                    Click="btnLimpiarCarteraColectiva_Click"
                                    Style="{StaticResource btnLimpiar}"
                                    ToolTipService.ToolTip="Limpiar" />
                            </Grid>
                        </Telerik:DataFormDataField>
                    </Grid>

                    <Button
                        Grid.Row="0"
                        Grid.Column="1"
                        Width="150"
                        VerticalAlignment="Bottom"
                        Click="btnConsultar_Click"
                        Content="Consultar" />
                </Grid>
                <Grid
                    Grid.Column="1"
                    HorizontalAlignment="Stretch"
                    Visibility="{Binding DataContext.HabilitarFiltrosOrdenesRecibo, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource VisibilityConverter}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="0"
                        Label="Sucursal"
                        LabelPosition="Above">
                        <ComboBox
                            DisplayMemberPath="Descripcion"
                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[SUCURSAL_PLUS], RelativeSource={RelativeSource AncestorType=UserControl}}"
                            SelectedValue="{Binding strSUCURSAL_PLUS, Mode=TwoWay}"
                            SelectedValuePath="Retorno" />
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="1"
                        Label="Receptor"
                        LabelPosition="Above">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TextBox
                                Grid.Column="0"
                                Background="{Binding DataContext.FondoTextoBuscadoresHabilitado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                MouseLeftButtonDown="txtReceptor2_MouseLeftButtonDown"
                                Text="{Binding DataContext.NombreReceptor, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                            <Controles:BuscadorGenericoListaButon
                                x:Name="ctlBuscadorReceptor2"
                                Grid.Column="1"
                                Margin="2,0,0,0"
                                HorizontalAlignment="Left"
                                CampoBusqueda="receptores"
                                EstadoItem="A"
                                TipoItem="receptoresusuario"
                                finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                            <Button
                                Grid.Column="2"
                                Margin="2,0,0,0"
                                VerticalAlignment="Bottom"
                                Click="btnLimpiarReceptor_Click_1"
                                Style="{StaticResource btnLimpiar}"
                                ToolTipService.ToolTip="Limpiar" />
                        </Grid>
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="2"
                        HorizontalAlignment="Stretch"
                        Label="Estado"
                        LabelPosition="Above">
                        <ComboBox
                            DisplayMemberPath="Descripcion"
                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[ESTADOS_TESORERO], RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                            SelectedValue="{Binding strEstado, Mode=TwoWay}"
                            SelectedValuePath="Retorno" />
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="3"
                        Label="Fecha orden"
                        LabelPosition="Above">
                        <A2UTILGEN:A2DatePicker SelectedDate="{Binding Fecha, Mode=TwoWay}" />
                    </Telerik:DataFormDataField>
                    <Button
                        Grid.Row="0"
                        Grid.Column="4"
                        Width="150"
                        VerticalAlignment="Bottom"
                        Click="btnConsultar_Click"
                        Content="Consultar" />
                </Grid>
                <Grid
                    Grid.Column="1"
                    HorizontalAlignment="Stretch"
                    Visibility="{Binding DataContext.HabilitarFiltrosOrdenesFondos, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource VisibilityConverter}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20*" />
                        <ColumnDefinition Width="20*" />
                        <ColumnDefinition Width="30*" />
                        <ColumnDefinition Width="15*" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="15*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>

                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="0"
                        Label="Sucursal"
                        LabelPosition="Above">
                        <ComboBox
                            DisplayMemberPath="Descripcion"
                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[SUCURSAL_PLUS], RelativeSource={RelativeSource AncestorType=UserControl}}"
                            SelectedValue="{Binding strSUCURSAL_PLUS, Mode=TwoWay}"
                            SelectedValuePath="Retorno" />
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="1"
                        Label="Receptor"
                        LabelPosition="Above">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TextBox
                                Grid.Column="0"
                                Background="{Binding DataContext.FondoTextoBuscadoresHabilitado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                MouseLeftButtonDown="txtReceptor3_MouseLeftButtonDown"
                                Text="{Binding DataContext.NombreReceptor, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                            <Controles:BuscadorGenericoListaButon
                                x:Name="ctlBuscadorReceptor3"
                                Grid.Column="1"
                                Margin="2,0,0,0"
                                HorizontalAlignment="Left"
                                CampoBusqueda="receptores"
                                EstadoItem="A"
                                TipoItem="receptoresusuario"
                                finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                            <Button
                                Grid.Column="2"
                                Margin="2,0,0,0"
                                VerticalAlignment="Bottom"
                                Click="btnLimpiarReceptor_Click_1"
                                Style="{StaticResource btnLimpiar}"
                                ToolTipService.ToolTip="Limpiar" />
                        </Grid>
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="2"
                        IsEnabled="{Binding HabilitarFormaPago}"
                        Label="Forma Pago"
                        LabelPosition="Above">
                        <ComboBox
                            DisplayMemberPath="Descripcion"
                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[TIPOPAGOPLUS], RelativeSource={RelativeSource AncestorType=UserControl}}"
                            SelectedValue="{Binding strTipoPagoPlus, Mode=TwoWay}"
                            SelectedValuePath="Retorno" />
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="3"
                        Label="Fecha orden"
                        LabelPosition="Above">
                        <A2UTILGEN:A2DatePicker SelectedDate="{Binding Fecha, Mode=TwoWay}" />
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="4"
                        Label="Fondos"
                        LabelPosition="Above">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <TextBox
                                Width="200"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Center"
                                Background="{Binding DataContext.FondoTextoBuscadoresHabilitado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                MouseLeftButtonDown="txtCartera3_MouseLeftButtonDown"
                                Text="{Binding DataContext.DescripcionCarteraColectiva, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                            <Controles:BuscadorGenericoListaButon
                                x:Name="ctlBuscadorCartera3"
                                Grid.Column="1"
                                Margin="2,0,0,0"
                                HorizontalAlignment="Left"
                                CampoBusqueda="carterascolectivas"
                                Condicion1="{Binding DataContext.FechaConvertida, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                Condicion2="{Binding DataContext.strEstado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                EstadoItem="A"
                                TipoItem="FONDOSTESORERO"
                                finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                            <Button
                                Grid.Column="2"
                                Margin="2,0,0,0"
                                VerticalAlignment="Bottom"
                                Click="btnLimpiarCarteraColectiva_Click"
                                Style="{StaticResource btnLimpiar}"
                                ToolTipService.ToolTip="Limpiar" />
                        </Grid>
                    </Telerik:DataFormDataField>
                    <Telerik:DataFormDataField
                        Grid.Row="0"
                        Grid.Column="5"
                        HorizontalAlignment="Stretch"
                        Label="Estado"
                        LabelPosition="Above">
                        <ComboBox
                            DisplayMemberPath="Descripcion"
                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[ESTADOS_TESORERO], RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                            SelectedValue="{Binding strEstado, Mode=TwoWay}"
                            SelectedValuePath="Retorno" />
                    </Telerik:DataFormDataField>
                    <Button
                        Grid.Row="0"
                        Grid.Column="6"
                        Width="150"
                        VerticalAlignment="Bottom"
                        Click="btnConsultar_Click"
                        Content="Consultar" />
                </Grid>
                <!--  GRIDS Por Tipo Negocio  -->
                <Grid
                    Grid.Row="2"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Margin="0,5,0,0"
                    HorizontalAlignment="Stretch">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <StackPanel Grid.ColumnSpan="2" Orientation="Vertical">
                        <TextBlock
                            Margin="6,0,0,0"
                            Style="{StaticResource Title2}"
                            Text="Documentos" />
                        <TextBlock Margin="7,0,0,0" Text="(*) Campos por los cuales se puede agrupar." />
                    </StackPanel>

                    <Telerik:DataFormDataField
                        Grid.Row="1"
                        Grid.Column="0"
                        Label="Seleccionar Todos">
                        <CheckBox
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            IsChecked="{Binding SeleccionarTodos, Mode=TwoWay}" />
                    </Telerik:DataFormDataField>
                    <!--  Cheques  -->
                    <Border
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="5,0,0,0"
                        HorizontalAlignment="Stretch"
                        Style="{StaticResource Contenedor_Paginador}"
                        Visibility="{Binding VerTabCheque}">
                        <A2UTILGEN:A2DataPager
                            x:Name="datapager"
                            PageSize="10"
                            Source="{Binding Items, ElementName=dgppal}" />
                    </Border>
                    <A2UTILGEN:A2GridView
                        Name="dgppal"
                        Grid.Row="3"
                        Grid.ColumnSpan="2"
                        Margin="5,0,0,0"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        AutoGenerateColumns="False"
                        CanUserDeleteRows="False"
                        CanUserInsertRows="False"
                        CanUserSortColumns="True"
                        CanUserSortGroups="True"
                        Filtered="dgppal_FilterChanged"
                        FrozenColumnCount="2"
                        IsFilteringAllowed="True"
                        ItemsSource="{Binding ListaResultadosDocumentos_Paged}"
                        ScrollViewer.HorizontalScrollBarVisibility="Auto"
                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                        SelectedItem="{Binding SelectedDocumentos, Mode=TwoWay}"
                        ShowGroupPanel="True"
                        Visibility="{Binding VerTabCheque}">
                        <A2UTILGEN:A2GridView.Columns>
                            <Telerik:GridViewDataColumn Header="Generar">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <CheckBox IsChecked="{Binding Generar, Mode=TwoWay}" IsEnabled="{Binding logHabilitarGenerar, Converter={StaticResource InBooleanConverter}}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn
                                Width="120"
                                DataMemberBinding="{Binding lngIDEncabezado}"
                                Header="Nro. Orden *"
                                SortMemberPath="lngIDEncabezado">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Button
                                                VerticalAlignment="Center"
                                                Click="VerOrden"
                                                Style="{StaticResource HyperlinkLikeButton}"
                                                Tag="{Binding}">
                                                <TextBlock
                                                    FontFamily="Arial Black"
                                                    FontSize="12"
                                                    FontStyle="Normal"
                                                    FontWeight="Bold"
                                                    Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                    Text="{Binding strTextoNroOrden, Mode=TwoWay}" />
                                            </Button>
                                        </StackPanel>
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn
                                Width="80"
                                DataMemberBinding="{Binding lngID}"
                                Header="Detalle *"
                                SortMemberPath="lngID">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal">
                                            <Button
                                                VerticalAlignment="Center"
                                                Click="EditarDetalle"
                                                Style="{StaticResource HyperlinkLikeButton}"
                                                Tag="{Binding}">
                                                <TextBlock
                                                    FontFamily="Arial Black"
                                                    FontSize="12"
                                                    FontStyle="Normal"
                                                    FontWeight="Bold"
                                                    Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                    Text="{Binding lngID, Mode=TwoWay}" />
                                            </Button>
                                        </StackPanel>
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn Header="Adjunto">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <Button
                                            VerticalAlignment="Center"
                                            Click="Button_Click_2"
                                            Content="{Binding strAdjunto}"
                                            Style="{StaticResource HyperlinkLikeButton}"
                                            Tag="{Binding urlAdjunto}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn Header="Consecutivo">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBox
                                            Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                            IsEnabled="False"
                                            Text="{Binding strConsecutivo, Mode=TwoWay}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn Header="Registrado Por">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBox
                                            Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                            IsEnabled="False"
                                            Text="{Binding strCodigoReceptor, Mode=TwoWay}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn
                                DataMemberBinding="{Binding strIDComitente}"
                                FilterMemberPath="strIDComitente"
                                Header="Código Cliente *"
                                SortMemberPath="strIDComitente">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBox
                                            Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                            IsEnabled="False"
                                            Text="{Binding strIDComitente, Mode=TwoWay}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn
                                DataMemberBinding="{Binding strNombreCliente}"
                                FilterMemberPath="strNombreCliente"
                                Header="Cliente *"
                                SortMemberPath="strNombreCliente">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBox
                                            Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                            IsEnabled="False"
                                            Text="{Binding strNombreCliente, Mode=TwoWay}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
                            <Telerik:GridViewDataColumn Header="Monto">
                                <Telerik:GridViewDataColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBox
                                            Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                            IsEnabled="False"
                                            Text="{Binding curValor, Mode=TwoWay, StringFormat='\{0:c2\}'}" />
                                    </DataTemplate>
                                </Telerik:GridViewDataColumn.CellTemplate>
                            </Telerik:GridViewDataColumn>
