                            Label="{Binding DataContext.TituloSaldoBancoOrigen, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            LabelPosition="Above"
                            Visibility="{Binding VerBanco}">
                            <ControlesPLUS:SaldoBancoView
                                BancoOYDSeleccionado="{Binding DataContext.IdBanco, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                ConsultarSaldoBanco="{Binding DataContext.ConsultarSaldoBanco, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                HabilitarSaldoBanco="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                        </Telerik:DataFormDataField>

                        <Telerik:DataFormDataField
                            Label="Banco fondos"
                            LabelPosition="Above"
                            Visibility="{Binding VerBancoFondos}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBox
                                    Grid.Column="0"
                                    Width="200"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Center"
                                    IsReadOnly="True"
                                    MouseLeftButtonDown="txtBanco2_MouseLeftButtonDown"
                                    Text="{Binding DataContext.DescripcionBancoFondo, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                <Controles:BuscadorGenericoListaButon
                                    x:Name="ctlBuscadorCuentasFondos"
                                    Grid.Column="1"
                                    Margin="2,0,0,0"
                                    CampoBusqueda="banconacionalfondos"
                                    EstadoItem="A"
                                    Etiqueta="Lista de Bancos fondos"
                                    GotFocus="BuscadorCuentasBancariasCarteras_GotFocus_1"
                                    TipoItem="cuentasbancariascartera"
                                    finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                                <Button
                                    Grid.Column="2"
                                    Margin="2,0,0,0"
                                    VerticalAlignment="Bottom"
                                    Click="btnLimpiarBanco2_Click_1"
                                    Cursor="Hand"
                                    Style="{StaticResource btnLimpiar}"
                                    ToolTipService.ToolTip="Limpiar" />
                            </Grid>
                        </Telerik:DataFormDataField>

                        <Telerik:DataFormDataField
                            Label="Banco fondos Destino"
                            LabelPosition="Above"
                            Visibility="{Binding VerBancoFondosDestino}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBox
                                    Grid.Column="0"
                                    Width="200"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Center"
                                    IsReadOnly="True"
                                    MouseLeftButtonDown="txtBanco2_MouseLeftButtonDown"
                                    Text="{Binding DataContext.DescripcionBancoFondoDestino, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                <Controles:BuscadorGenericoListaButon
                                    x:Name="ctlBuscadorCuentasFondosDestino"
                                    Grid.Column="1"
                                    Margin="2,0,0,0"
                                    CampoBusqueda="banconacionalfondos"
                                    EstadoItem="A"
                                    Etiqueta="Lista de Bancos fondos"
                                    GotFocus="BuscadorCuentasBancariasCarteras_GotFocus_2"
                                    TipoItem="cuentasbancariascartera"
                                    finalizoBusqueda="BuscadorGenerico_finalizoBusqueda2" />
                                <Button
                                    Grid.Column="2"
                                    Margin="2,0,0,0"
                                    VerticalAlignment="Bottom"
                                    Click="btnLimpiarBanco2_Click_Destino"
                                    Cursor="Hand"
                                    Style="{StaticResource btnLimpiar}"
                                    ToolTipService.ToolTip="Limpiar" />
                            </Grid>
                        </Telerik:DataFormDataField>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Telerik:DataFormDataField
                            Grid.Row="1"
                            Grid.Column="0"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            Label="{Binding DataContext.TituloConsecutivoDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            LabelPosition="Above"
                            Visibility="{Binding DataContext.VerConsecutivosNOTADestino, RelativeSource={RelativeSource AncestorType=UserControl}}">
                            <ComboBox
                                Width="150"
                                DisplayMemberPath="Descripcion"
                                ItemsSource="{Binding DataContext.ListaConsecutivosNotasDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                SelectedValue="{Binding DataContext.strConsecutivoNOTAS_PLUS_Destino, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                SelectedValuePath="ID" />
                        </Telerik:DataFormDataField>

                        <Telerik:DataFormDataField
                            Grid.Row="1"
                            Grid.Column="0"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            Label="{Binding DataContext.TituloConsecutivoDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            LabelPosition="Above"
                            Visibility="{Binding DataContext.VerConsecutivosEgresoDestino, RelativeSource={RelativeSource AncestorType=UserControl}}">
                            <ComboBox
                                Width="150"
                                DisplayMemberPath="Descripcion"
                                ItemsSource="{Binding DataContext.ListaConsecutivosEgresosDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                SelectedValue="{Binding DataContext.strConsecutivoCE_PLUS_Destino, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                SelectedValuePath="ID" />
                        </Telerik:DataFormDataField>

                        <Telerik:DataFormDataField
                            Grid.Row="1"
                            Grid.Column="0"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            Label="{Binding DataContext.TituloConsecutivoDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            LabelPosition="Above"
                            Visibility="{Binding DataContext.VerConsecutivosReciboDestino, RelativeSource={RelativeSource AncestorType=UserControl}}">
                            <ComboBox
                                Width="150"
                                VerticalAlignment="Bottom"
                                DisplayMemberPath="Descripcion"
                                ItemsSource="{Binding DataContext.ListaConsecutivosRecibosDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                SelectedValue="{Binding DataContext.strConsecutivoRC_PLUS_Destino, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                SelectedValuePath="ID" />
                        </Telerik:DataFormDataField>

                        <Telerik:DataFormDataField
                            Grid.Row="1"
                            Grid.Column="1"
                            IsEnabled="{Binding DataContext.HabilitarGeneracionDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            Label="{Binding DataContext.TituloBancoDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            LabelPosition="Above"
                            Visibility="{Binding DataContext.VerBancoFondosOYD, RelativeSource={RelativeSource AncestorType=UserControl}}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <TextBox
                                    Grid.Column="0"
                                    Width="200"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Center"
                                    IsReadOnly="True"
                                    MouseLeftButtonDown="txtBanco3_MouseLeftButtonDown"
                                    Text="{Binding DataContext.DescripcionBanco_FondosOYD, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                <Controles:BuscadorGenericoListaButon
                                    x:Name="CuentasBancariasDestino"
                                    Grid.Column="1"
                                    Margin="2,0,0,0"
                                    Agrupamiento="{Binding DataContext.AgrupamientoFirmaDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                    CampoBusqueda="cuentasbancariasdestino"
                                    Condicion1="{Binding DataContext.CarteraColectivaDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                    EstadoItem="A"
                                    Etiqueta="Lista de Bancos"
                                    IsEnabled="{Binding DataContext.HabilitarGeneracionDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                    TipoItem="cuentasbancarias_tesorero"
                                    finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                                <Button
                                    Grid.Column="2"
                                    Margin="2,0,0,0"
                                    Click="btnLimpiarBanco3_Click_1"
                                    Cursor="Hand"
                                    Style="{StaticResource btnLimpiar}"
                                    ToolTipService.ToolTip="Limpiar" />
                            </Grid>
                        </Telerik:DataFormDataField>

                        <Telerik:DataFormDataField
                            Grid.Row="1"
                            Grid.Column="2"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            Label="{Binding DataContext.TituloSaldoBancoDestino, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            LabelPosition="Above"
                            Visibility="{Binding VerBancoFondosOYD}">
                            <ControlesPLUS:SaldoBancoView
                                BancoOYDSeleccionado="{Binding DataContext.IdBanco_FondosOYD, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                ConsultarSaldoBanco="{Binding DataContext.ConsultarSaldoBanco_FondosOYD, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                HabilitarSaldoBanco="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                        </Telerik:DataFormDataField>
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Telerik:DataFormDataField
                            Grid.Row="2"
                            Grid.Column="0"
                            Label="Total a Generar"
                            LabelPosition="Above">
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="1"
                                Text="{Binding DataContext.TotalGenerar, RelativeSource={RelativeSource AncestorType=UserControl}, StringFormat='$ {0:#,#0.00}'}" />
                        </Telerik:DataFormDataField>
                    </StackPanel>
                </StackPanel>
                <Grid
                    x:Name="StackBotonesNormal"
                    Grid.Row="5"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    HorizontalAlignment="Right"
                    Visibility="{Binding DataContext.HabilitarCamposFondosOYD, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource InVisibilityConverter}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                        <Button
                            MaxWidth="200"
                            Margin="0,0,0,5"
                            VerticalAlignment="Bottom"
                            Click="btnGenerar_Click"
                            Content="{Binding DataContext.strTextoGenerarDocumento, RelativeSource={RelativeSource AncestorType=UserControl}}"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                        <Button
                            MaxWidth="200"
                            Margin="0,0,0,5"
                            VerticalAlignment="Bottom"
                            Click="btnRechazar_Click"
                            Content="Rechazar Documentos"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                    </StackPanel>
                </Grid>
                <Grid
                    x:Name="StackBotonesFondosOYD"
                    Grid.Row="5"
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    HorizontalAlignment="Right"
                    Visibility="{Binding DataContext.HabilitarCamposFondosOYD, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource VisibilityConverter}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <StackPanel
                        Margin="5"
                        HorizontalAlignment="Right"
                        Orientation="Horizontal">
                        <Button
                            Width="200"
                            VerticalAlignment="Bottom"
                            Click="btnGenerar_Click"
                            Content="Generar Documentos"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                        <Button
                            MaxWidth="200"
                            Margin="2,0,0,0"
                            VerticalAlignment="Bottom"
                            Click="btnRechazar_Click"
                            Content="Rechazar Documentos"
                            IsEnabled="{Binding DataContext.HabilitarGenerarDctos, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                    </StackPanel>
                </Grid>
            </Grid>
        </Border>
    </Telerik:RadBusyIndicator>
</UserControl>
