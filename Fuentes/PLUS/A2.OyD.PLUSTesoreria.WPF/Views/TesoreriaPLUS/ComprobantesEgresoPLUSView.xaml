                                                                                x:Name="btnLimpiarCiudad"
                                                                                Margin="2,0,0,0"
                                                                                VerticalAlignment="Bottom"
                                                                                Click="btnLimpiarCiudad_Click"
                                                                                Cursor="Hand"
                                                                                IsEnabled="{Binding DataContext.HabilitarCamposDireccion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                                Style="{StaticResource btnLimpiar}"
                                                                                ToolTipService.ToolTip="Limpiar" />
                                                                        </StackPanel>
                                                                    </StackPanel>

                                                                    <StackPanel
                                                                        Grid.Row="5"
                                                                        Width="180"
                                                                        HorizontalAlignment="Left"
                                                                        Visibility="{Binding DataContext.VerCamposOpcionalesLlevarDireccion, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}">
                                                                        <TextBlock Style="{StaticResource LabelTextBlockForm}" Text="Sector" />
                                                                        <TextBox
                                                                            Width="170"
                                                                            IsEnabled="{Binding DataContext.HabilitarCamposDireccion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                            MaxLength="50"
                                                                            Text="{Binding DataContext.strSectorInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                            Visibility="{Binding DataContext.VerCamposOpcionalesLlevarDireccion, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                                                    </StackPanel>
                                                                </Grid>
                                                            </Grid>
                                                            <!--  TERMINA GRID DERECHO 1  -->

                                                            <!--  GRID DERECHO 2  -->
                                                            <Grid
                                                                Grid.Column="2"
                                                                HorizontalAlignment="Stretch"
                                                                VerticalAlignment="Stretch"
                                                                Visibility="{Binding DataContext.OtrosChecked, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource InVisibilityConverter}}">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition Height="auto" />
                                                                </Grid.RowDefinitions>
                                                                <Grid Grid.Row="0">
                                                                    <Grid.RowDefinitions>
                                                                        <RowDefinition Height="auto" />
                                                                        <RowDefinition Height="auto" />
                                                                        <RowDefinition Height="auto" />
                                                                        <RowDefinition Height="auto" />
                                                                        <RowDefinition Height="auto" />
                                                                        <RowDefinition Height="auto" />
                                                                        <RowDefinition Height="auto" />
                                                                    </Grid.RowDefinitions>
                                                                    <Grid.ColumnDefinitions>
                                                                        <ColumnDefinition Width="Auto" />
                                                                    </Grid.ColumnDefinitions>

                                                                    <StackPanel
                                                                        Grid.Row="0"
                                                                        Grid.Column="1"
                                                                        Width="180"
                                                                        HorizontalAlignment="Left"
                                                                        Visibility="{Binding DataContext.VerConsignarCuenta, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}">
                                                                        <TextBlock Style="{StaticResource LabelTextBlockForm}" Text="Banco" />
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <StackPanel Background="{Binding DataContext.FondoTextoBuscadores, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                                                                <TextBox
                                                                                    Name="txtDescripcionBanco"
                                                                                    Width="122"
                                                                                    Margin="5,0,0,0"
                                                                                    HorizontalAlignment="Stretch"
                                                                                    VerticalAlignment="Center"
                                                                                    IsReadOnly="True"
                                                                                    MouseLeftButtonDown="txtDescripcionBanco_MouseLeftButtonDown"
                                                                                    Text="{Binding DataContext.strDescripcionBancoInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                                                            </StackPanel>
                                                                            <Controles:BuscadorGenericoListaButon
                                                                                x:Name="ctlBuscadorBanco"
                                                                                Margin="2,0,0,0"
                                                                                VerticalAlignment="Top"
                                                                                CampoBusqueda="bancosnacionalesinstrucciones"
                                                                                EstadoItem="A"
                                                                                Etiqueta="Lista de Bancos"
                                                                                IsEnabled="{Binding DataContext.HabilitarCamposConsignarInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                                TipoItem="bancosnacionales"
                                                                                finalizoBusqueda="BuscadorGenerico_finalizoBusqueda" />
                                                                            <Button
                                                                                x:Name="btnLimpiarBanco"
                                                                                Margin="2,0,0,0"
                                                                                VerticalAlignment="Bottom"
                                                                                Click="btnLimpiarBanco_Click"
                                                                                Cursor="Hand"
                                                                                IsEnabled="{Binding DataContext.HabilitarCamposConsignarInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                                Style="{StaticResource btnLimpiar}"
                                                                                ToolTipService.ToolTip="Limpiar" />
                                                                        </StackPanel>
                                                                    </StackPanel>

                                                                    <StackPanel
                                                                        Grid.Row="1"
                                                                        Width="180"
                                                                        HorizontalAlignment="Left"
                                                                        Visibility="{Binding DataContext.VerConsignarCuenta, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}">
                                                                        <TextBlock Style="{StaticResource LabelTextBlockForm}" Text="Tipo cuenta" />
                                                                        <ComboBox
                                                                            Name="cboTipoCuenta"
                                                                            Width="170"
                                                                            DisplayMemberPath="Descripcion"
                                                                            IsEnabled="{Binding DataContext.HabilitarCamposConsignarInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[TIPOCUENTABANCARIA], RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                            SelectedItem="{Binding DataContext.strTipoCuentaSeleccionadoInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                            SelectedValue="{Binding DataContext.strValorTipoCuentaInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                            SelectedValuePath="Retorno"
                                                                            Visibility="{Binding DataContext.VerConsignarCuenta, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                                                    </StackPanel>

                                                                    <StackPanel
                                                                        Grid.Row="2"
                                                                        Width="180"
                                                                        HorizontalAlignment="Left"
                                                                        Visibility="{Binding DataContext.VerConsignarCuenta, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}">
                                                                        <TextBlock Style="{StaticResource LabelTextBlockForm}" Text="Número de cuenta" />
                                                                        <TextBox
                                                                            Width="170"
                                                                            IsEnabled="{Binding DataContext.HabilitarCamposConsignarInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                            MaxLength="50"
                                                                            Text="{Binding DataContext.strCuentaInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                            Visibility="{Binding DataContext.VerConsignarCuenta, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}" />
                                                                    </StackPanel>
                                                                </Grid>
                                                            </Grid>

                                                            <Grid
                                                                Grid.Column="1"
                                                                Grid.ColumnSpan="2"
                                                                HorizontalAlignment="Stretch"
                                                                Visibility="{Binding DataContext.OtrosChecked, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource VisibilityConverter}}">
                                                                <StackPanel HorizontalAlignment="Left">
                                                                    <TextBlock Style="{StaticResource LabelTextBlockForm}" Text="Descripción" />
                                                                    <TextBox
                                                                        Name="txtOtros"
                                                                        Width="600"
                                                                        IsEnabled="{Binding DataContext.HabilitarOtrosInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                        MaxLength="8000"
                                                                        Text="{Binding DataContext.strOtrosInstrucciones, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                        TextWrapping="Wrap" />
                                                                </StackPanel>
                                                            </Grid>
                                                        </Grid>
                                                    </TabItem>

                                                    <!--  TAB OBSERVACIONES  -->
                                                    <TabItem
                                                        x:Name="tabObservaciones"
                                                        HorizontalAlignment="Stretch"
                                                        VerticalAlignment="Stretch"
                                                        Header="Observaciones">
                                                        <Grid>
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition Height="auto" />
                                                            </Grid.RowDefinitions>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="620" />
                                                            </Grid.ColumnDefinitions>
                                                            <StackPanel Grid.Row="0" Grid.Column="0">
                                                                <TextBlock Text="Observaciones" />
                                                                <TextBox
                                                                    Name="txtConcepto"
                                                                    Width="600"
                                                                    Height="50"
                                                                    IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                    MaxLength="250"
                                                                    Text="{Binding strObservaciones, Mode=TwoWay}"
                                                                    TextWrapping="Wrap"
                                                                    VerticalScrollBarVisibility="Auto" />
                                                            </StackPanel>
                                                        </Grid>
                                                        <!--  TAB INSTRUCCIONES  -->
                                                    </TabItem>
                                                </TabControl>
                                            </Border>
                                        </Telerik:RadBusyIndicator>
                                    </Grid>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>
                </ScrollViewer>
            </Border>
        </Grid>
    </Telerik:RadBusyIndicator>
</UserControl>