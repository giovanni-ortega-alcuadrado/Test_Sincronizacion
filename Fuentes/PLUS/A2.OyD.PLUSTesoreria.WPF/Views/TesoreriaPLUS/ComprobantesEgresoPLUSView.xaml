<UserControl
    x:Class="ComprobantesEgresoPLUSView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:A2="clr-namespace:A2ControlMenu;assembly=A2ControlMenu.WPF"
    xmlns:A2UTILGEN="clr-namespace:A2Utilidades;assembly=A2.Controles.Utilidades.WPF"
    xmlns:A2utils="clr-namespace:A2ControlMenu;assembly=A2ControlMenu.WPF"
    xmlns:Controles="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:ControlesPLUS="clr-namespace:A2OYDPLUSUtilidades;assembly=A2.OyD.PLUSUtilidades.WPF"
    xmlns:Telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:c2="http://schemas.componentone.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:documentos="clr-namespace:A2DocumentosWPF;assembly=A2.Documentos.WPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:src="clr-namespace:A2OYDPLUSTesoreria"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    HorizontalAlignment="Stretch"
    VerticalAlignment="Stretch"
    Unloaded="ComprobantesEgresoPLUSView_Unload"
    mc:Ignorable="d">




    <Telerik:RadBusyIndicator
        x:Name="myBusyIndicator"
        HorizontalAlignment="Stretch"
        VerticalAlignment="Stretch"
        BusyContent="{Binding DescripcionBusy}"
        DisplayAfter="0"
        IsBusy="{Binding IsBusy}">
        <Grid
            x:Name="LayoutRoot"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch">
            <Grid.Resources>
                <src:A2UtilsViewModel x:Key="A2VM" />
                <src:clsLiderConverter x:Key="cvLider" />
                <src:VisibilityConverter x:Key="VisibilityConverter" />
                <src:clsVisibilityConverter x:Key="clsVisibilityConverter" />
                <src:InVisibilityConverter x:Key="InVisibilityConverter" />
                <src:ColorFilaConverter x:Key="ColorFilaConverter" />
            </Grid.Resources>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>


            <Border
                Grid.Row="0"
                Grid.Column="1"
                Padding="0,0,0,10"
                BorderBrush="Black"
                BorderThickness="0,0,0,1">
                <Grid
                    Grid.Row="0"
                    Grid.Column="1"
                    HorizontalAlignment="Stretch">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="40" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <Button
                        Grid.Row="0"
                        MinWidth="120"
                        HorizontalAlignment="Right"
                        Click="Cerrar"
                        Content="Cerrar"
                        Visibility="{Binding MostrarControlMenuGuardar, Converter={StaticResource InVisibilityConverter}}" />

                    <A2utils:ControlMenuA2
                        x:Name="cm"
                        Grid.Row="1"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch"
                        DF="{Binding df}"
                        DescripcionFiltro="{Binding DescripcionFiltroVM, Mode=TwoWay}"
                        DicBotonesMenu="{Binding DicBotonesMenuVM, Mode=TwoWay}"
                        EstadoMenu="{Binding EstadoMenuVM, Mode=TwoWay}"
                        EventoCancelarGrabacion="CancelarEditarRegistro_Click"
                        EventoConfirmarGrabacion="cm_EventoConfirmarGrabacion"
                        EventoNuevoRegistro="cm_EventoNuevoRegistro_1"
                        Filtro="{Binding FiltroVM, Mode=TwoWay}"
                        IsItemValid="True"
                        ItemGridHyperlink="{Binding ItemSeleccionadoHyperlink, Mode=TwoWay}"
                        ManejaMakerAndChecker="False"
                        Mensaje="{Binding MensajeMVVM, Mode=TwoWay}"
                        MensajeError="{Binding ErrorForma}"
                        Modo="{Binding ModoMVVM, Mode=TwoWay}"
                        ObjetoSeleccionado="{Binding TesoreriaOrdenesPlusCE_Selected}"
                        PreguntarConfirmacion="False"
                        TituloVista=" Órdenes de Pago"
                        TituloVistaAdicional="registros"
                        TituloVista_CambioSelected="{Binding TesoreriaOrdenesPlusCE_Selected.HasChanges}"
                        TituloVista_CantidadRegistros="{Binding ListaTesoreriaOrdenesPlusCE.Count}"
                        VisibilidadRefrescarCombos="True" />

                    <StackPanel
                        Grid.Row="2"
                        Margin="0,5,0,0"
                        HorizontalAlignment="Right"
                        Orientation="Horizontal"
                        Visibility="{Binding MostrarControlMenuGuardar, Converter={StaticResource VisibilityConverter}}">
                        <StackPanel
                            HorizontalAlignment="Right"
                            Orientation="Horizontal"
                            Visibility="{Binding Editando, Converter={StaticResource InVisibilityConverter}}">
                            <Button
                                HorizontalAlignment="Right"
                                Click="Button_Click"
                                Content="Ord. programada"
                                Style="{StaticResource ControlButtonSecundaryStyle}"
                                Visibility="{Binding HabilitarProgramaciones, Converter={StaticResource VisibilityConverter}}" />
                            <Button
                                HorizontalAlignment="Right"
                                Click="Button_Click_1"
                                Content="Duplicar"
                                Style="{StaticResource ControlButtonSecundaryStyle}"
                                Visibility="{Binding MostrarBotonDuplicar, Converter={StaticResource VisibilityConverter}}" />
                        </StackPanel>
                    </StackPanel>
                </Grid>
            </Border>


            <Border
                Grid.Row="2"
                Grid.Column="1"
                Style="{StaticResource Contenedor_Estilo}">

                <ScrollViewer
                    x:Name="ScrollForma"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    HorizontalScrollBarVisibility="Auto"
                    VerticalScrollBarVisibility="Auto">
                    <Grid
                        x:Name="grdGridForma"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch">

                        <Border
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            BorderThickness="1">
                            <StackPanel
                                x:Name="Buscar"
                                Grid.Column="1"
                                Height="Auto"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">
                                <ListBox
                                    x:Name="lstVistaDatos"
                                    ItemsSource="{Binding ListaDatos}"
                                    SelectedItem="{Binding VistaSeleccionada, Mode=TwoWay}">
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <StackPanel Orientation="Horizontal" />
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                </ListBox>
                                <Grid HorizontalAlignment="Stretch">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <Button
                                        x:Name="btnRefrescarPantalla"
                                        Grid.Column="0"
                                        Width="25"
                                        Height="25"
                                        Click="btnRefrescarPantalla_Click"
                                        Cursor="Hand"
                                        Style="{StaticResource btnRefrescar}"
                                        ToolTipService.ToolTip="Refrescar" />
                                    <Border
                                        Grid.Column="2"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        Style="{StaticResource Contenedor_Paginador}">
                                        <A2UTILGEN:A2DataPager
                                            x:Name="datapager"
                                            Height="auto"
                                            PageSize="10"
                                            Source="{Binding Items, ElementName=dgppal}"
                                            Visibility="Visible" />
                                    </Border>
                                </Grid>
                                <A2UTILGEN:A2GridView
                                    x:Name="dgppal"
                                    Grid.Row="1"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Stretch"
                                    AutoGenerateColumns="False"
                                    IsReadOnly="True"
                                    ItemsSource="{Binding ListaTesoreriaOrdenesPlusCE_Paged}"
                                    ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                    ScrollViewer.VerticalScrollBarVisibility="Auto"
                                    SelectedItem="{Binding TesoreriaOrdenesPlusCE_Selected, Mode=TwoWay}"
                                    ValidatesOnDataErrors="None">
                                    <A2UTILGEN:A2GridView.Columns>
                                        <Telerik:GridViewDataColumn Header="Número orden">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button
                                                        VerticalAlignment="Center"
                                                        Command="{Binding DataContext.Hyperlink_CambiarAForma, RelativeSource={RelativeSource AncestorType=A2UTILGEN:A2GridView}}"
                                                        CommandParameter="{Binding}"
                                                        Style="{StaticResource HyperlinkLikeButton}">
                                                        <TextBlock Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}" Text="{Binding lngID, Mode=TwoWay}" />
                                                    </Button>
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Width="25">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button
                                                        Width="15"
                                                        Height="15"
                                                        Padding="0"
                                                        HorizontalAlignment="Center"
                                                        Click="Button_Click_2"
                                                        Cursor="Hand"
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        Style="{StaticResource btnTraza}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Tipo">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strTipo, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Nro Documento">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strNroDocumento, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Tipo Identificación">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strTipoIdentificacion, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Cliente">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strNombre, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Id Comitente">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strIDComitente, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Valor">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding curValor, Mode=TwoWay, StringFormat=C2}"
                                                        TextAlignment="Right" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Tipo producto">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strTipoProducto, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Receptor">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strCodigoReceptor, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Fecha documento">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding dtmDocumento, Mode=TwoWay, StringFormat='dd/MM/yyyy'}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Estado">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBox
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        IsEnabled="False"
                                                        Text="{Binding strEstado, Mode=TwoWay}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                        <Telerik:GridViewDataColumn Header="Documento autorización pago">
                                            <Telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <Button
                                                        VerticalAlignment="Center"
                                                        Click="Button_Click_3"
                                                        Content="{Binding strDescripcionURL}"
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        Style="{StaticResource HyperlinkLikeButton}"
                                                        Tag="{Binding strUrlArchivo}" />
                                                </DataTemplate>
                                            </Telerik:GridViewDataColumn.CellTemplate>
                                        </Telerik:GridViewDataColumn>
                                    </A2UTILGEN:A2GridView.Columns>
                                </A2UTILGEN:A2GridView>
                            </StackPanel>
                        </Border>
                        <Border
                            Grid.Row="1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch">
                            <Grid Grid.Row="1">
                                <!--  Dataform de búsqueda  -->
                                <Grid
                                    x:Name="GridBusqueda"
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    HorizontalAlignment="Stretch"
                                    VerticalAlignment="Top"
                                    DataContext="{Binding cb}"
                                    Visibility="{Binding DataContext.visBuscando, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <Telerik:DataFormDataField
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        VerticalAlignment="Top"
                                        Label="Número orden"
                                        LabelPosition="Above">


                                        <TextBox Text="{Binding lngID, Mode=TwoWay}" />
                                    </Telerik:DataFormDataField>

                                    <Telerik:DataFormDataField
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        VerticalAlignment="Top"
                                        Label="Estado">
                                        <ComboBox
                                            DisplayMemberPath="Descripcion"
                                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[ESTADOS_PLUS], RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                            SelectedValue="{Binding strEstado, Mode=TwoWay}"
                                            SelectedValuePath="Retorno" />
                                    </Telerik:DataFormDataField>

                                    <Telerik:DataFormDataField
                                        Grid.Row="0"
                                        Grid.Column="2"
                                        VerticalAlignment="Top"
                                        Label="Tipo producto"
                                        LabelPosition="Above">
                                        <ComboBox
                                            x:Name="cboTipoProductoEdicion"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DisplayMemberPath="Descripcion"
                                            ItemsSource="{Binding DataContext.DiccionarioCombosOYDPlus[TIPOPRODUCTOTESORERIA], RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                            SelectedValue="{Binding strTipoProducto, Mode=TwoWay}"
                                            SelectedValuePath="Retorno" />
                                    </Telerik:DataFormDataField>

                                    <Telerik:DataFormDataField
                                        Grid.Row="0"
                                        Grid.Column="3"
                                        VerticalAlignment="Top"
                                        Label="Receptor"
                                        LabelPosition="Above">
                                        <ComboBox
                                            x:Name="cboReceptoresEdicion"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            DisplayMemberPath="Nombre"
                                            ItemsSource="{Binding DataContext.ListaReceptoresUsuario, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                            SelectedValue="{Binding strCodigoReceptor, Mode=TwoWay}"
                                            SelectedValuePath="CodigoReceptor" />
                                    </Telerik:DataFormDataField>




                                    <Telerik:DataFormDataField
                                        Grid.Row="0"
                                        Grid.Column="4"
                                        VerticalAlignment="Top"
                                        Label="Comitente"
                                        LabelPosition="Above">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>

                                            <TextBox Text="{Binding ComitenteSeleccionadoBusqueda.IdComitente}" />
                                            <Controles:BuscadorClienteListaButon
                                                x:Name="Buscador_Cliente"
                                                Grid.Column="1"
                                                Margin="2,0,0,0"
                                                HorizontalAlignment="Right"
                                                VerticalAlignment="Top"
                                                Agrupamiento=""
                                                CampoBusqueda="IDComitente"
                                                EstadoComitente="A"
                                                Etiqueta="Lista de Clientes"
                                                TipoVinculacion="C"
                                                finalizoBusqueda="ctrlCliente_comitenteAsignado_1" />
                                            <Button
                                                x:Name="btnLimpiarClienteBusqueda"
                                                Grid.Column="2"
                                                Margin="2,0,0,0"
                                                VerticalAlignment="Top"
                                                Click="btnLimpiarClienteBusqueda_Click"
                                                Cursor="Hand"
                                                Style="{StaticResource btnLimpiar}"
                                                ToolTipService.ToolTip="Limpiar cliente" />
                                        </Grid>
                                    </Telerik:DataFormDataField>

                                    <StackPanel Grid.Column="5">
                                        <StackPanel
                                            Background="White"
                                            DataContext="{Binding DataContext.cb.ComitenteSeleccionadoBusqueda, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=OneWay}"
                                            Orientation="Vertical">
                                            <TextBlock Text="{Binding Nombre}" />
                                            <TextBlock Text="{Binding IdComitente}" />
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="{Binding TipoIdentificacion, StringFormat='\{0} '}" />
                                                <TextBlock Text="{Binding NroDocumento, StringFormat='N0'}" />
                                            </StackPanel>
                                            <TextBlock Text="{Binding Categoria}" />
                                            <TextBlock Text="{Binding Estado}" />
                                        </StackPanel>

                                        <StackPanel
                                            Background="White"
                                            DataContext="{Binding DataContext.cb.OrdenanteSeleccionado, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=OneWay}"
                                            Orientation="Vertical">
                                            <TextBlock Text="{Binding Nombre}" />
                                            <StackPanel Orientation="Horizontal">
                                                <TextBlock Text="{Binding TipoIdentificacion, StringFormat='\{0} '}" />
                                                <TextBlock Text="{Binding NroDocumento, StringFormat='N0'}" />
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                </Grid>

                                <!--  Dataform de edición  -->
                                <StackPanel
                                    Name="stackpEdicion"
                                    DataContext="{Binding TesoreriaOrdenesPlusCE_Selected}"
                                    Orientation="Vertical"
                                    Visibility="{Binding DataContext.visNavegando, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                    <A2UTILGEN:A2DataForm
                                        x:Name="df"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        AutoGenerateFields="False"
                                        CommandButtonsVisibility="None"
                                        CurrentItem="{Binding TesoreriaOrdenesPlusCE_DataForm}"
                                        Visibility="Collapsed" />

                                    <Grid
                                        x:Name="GridControlesIniciales"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Top">
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>

                                        <Telerik:RadBusyIndicator
                                            x:Name="myBusyIndicatorReceptor"
                                            Grid.Row="0"
                                            Grid.Column="0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"
                                            BusyContent="Cargando información receptor..."
                                            DisplayAfter="0"
                                            IsBusy="{Binding DataContext.IsBusyReceptor, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                            <Grid VerticalAlignment="Top">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="0"
                                                    Label="Nro. orden"
                                                    LabelPosition="Above">
                                                    <TextBox
                                                        x:Name="txtNroOrden"
                                                        IsEnabled="False"
                                                        Text="{Binding lngID}" />
                                                </Telerik:DataFormDataField>
                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="1"
                                                    Label="Estado de la orden"
                                                    LabelPosition="Above">
                                                    <TextBox
                                                        x:Name="linkEstado"
                                                        IsEnabled="False"
                                                        Text="{Binding strEstado}" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    x:Name="myBusyIndicatorArchivo"
                                                    Grid.Row="0"
                                                    Grid.Column="2"
                                                    Label="Carta de autorización"
                                                    LabelPosition="Above">
                                                    <documentos:A2SubirDocumento
                                                        x:Name="ctlSubirArchivo"
                                                        HorizontalAlignment="Left"
                                                        IsEnabled="{Binding DataContext.HabilitarEnEdicion, RelativeSource={RelativeSource AncestorType=UserControl}}" />
                                                </Telerik:DataFormDataField>
                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="3"
                                                    Label="Trazabilidad"
                                                    LabelPosition="Above">
                                                    <Button
                                                        Width="20"
                                                        Height="20"
                                                        HorizontalAlignment="Left"
                                                        Click="Button_Click_2"
                                                        Cursor="Hand"
                                                        Foreground="{Binding strColorTextoRegistro, Converter={StaticResource ColorFilaConverter}}"
                                                        Style="{StaticResource btnTraza}" />
                                                </Telerik:DataFormDataField>
                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="4"
                                                    Label="Documento autorización pago"
                                                    LabelPosition="Above">
                                                    <Button
                                                        VerticalAlignment="Center"
                                                        Click="Button_Click_3"
                                                        Content="{Binding strDescripcionURL}"
                                                        Style="{StaticResource HyperlinkLikeButton}"
                                                        Tag="{Binding strUrlArchivo}" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="1"
                                                    Grid.Column="0"
                                                    Grid.ColumnSpan="2"
                                                    Label="Receptor"
                                                    LabelPosition="Above">
                                                    <ComboBox
                                                        x:Name="cboReceptores"
                                                        DisplayMemberPath="Nombre"
                                                        IsEnabled="{Binding DataContext.HabilitarEncabezado, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                        ItemsSource="{Binding DataContext.ListaReceptoresUsuario, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                        SelectedValue="{Binding strCodigoReceptor, Mode=TwoWay}"
                                                        SelectedValuePath="CodigoReceptor" />
                                                </Telerik:DataFormDataField>
                                                <Telerik:DataFormDataField
                                                    Grid.Row="1"
                                                    Grid.Column="2"
                                                    Grid.ColumnSpan="2"
                                                    Label="Tipo Producto"
                                                    LabelPosition="Above">
                                                    <ComboBox
                                                        x:Name="cboTipoProducto"
                                                        DisplayMemberPath="Descripcion"
                                                        IsEnabled="{Binding DataContext.HabilitarTipoProducto, RelativeSource={RelativeSource AncestorType=UserControl}}"
