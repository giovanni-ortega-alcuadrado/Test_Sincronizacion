                                        Header="Nombre receptor"
                                        IsReadOnly="True" />
                                </A2UTILGEN:A2GridView.Columns>
                            </A2UTILGEN:A2GridView>
                        </StackPanel>
                    </TabItem>

                    <!--  Tab para mostrar la información de las ordenes cruzadas  -->
                    <TabItem
                        x:Name="tabItemOrdenesCruzadas"
                        Header="{Binding TextoTabOrdenCruzada, Source={StaticResource VMOrdenes}}"
                        Visibility="{Binding MostrarOrdenesCruzadas, Source={StaticResource VMOrdenes}}">
                        <StackPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <A2UTILGEN:A2GridView
                                x:Name="dgOrdenesCruzadas"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                AutoGenerateColumns="False"
                                CanUserDeleteRows="False"
                                CanUserInsertRows="False"
                                FrozenColumnCount="3"
                                IsReadOnly="True"
                                ItemsSource="{Binding ListaOrdenesCruzadas, Source={StaticResource VMOrdenes}}"
                                ScrollViewer.HorizontalScrollBarVisibility="Visible"
                                ScrollViewer.VerticalScrollBarVisibility="Visible"
                                SelectedItem="{Binding OrdenesCruzadasSelected, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                ValidatesOnDataErrors="None">
                                <A2UTILGEN:A2GridView.Columns>
                                    <Telerik:GridViewDataColumn
                                        DataMemberBinding="{Binding IDNroOrden}"
                                        Header="Nro orden"
                                        IsReadOnly="True" />
                                    <Telerik:GridViewDataColumn
                                        DataMemberBinding="{Binding NombreReceptorCruzada}"
                                        Header="Receptor"
                                        IsReadOnly="True" />
                                    <Telerik:GridViewDataColumn
                                        DataMemberBinding="{Binding NombreClienteCruzada}"
                                        Header="Cliente"
                                        IsReadOnly="True" />
                                    <Telerik:GridViewDataColumn
                                        DataMemberBinding="{Binding ValorOrdenCruzada, StringFormat='\{0:N4\}'}"
                                        Header="Cantidad"
                                        IsReadOnly="True" />

                                    <Telerik:GridViewDataColumn Header="Cruce activo">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox IsChecked="{Binding CruceActivo, Mode=TwoWay}" IsEnabled="False" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                </A2UTILGEN:A2GridView.Columns>
                            </A2UTILGEN:A2GridView>

                            <Grid Visibility="{Binding OrdenCruzada, Converter={StaticResource InVisibilityConverter}}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>

                                <Telerik:DataFormDataField
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    HorizontalAlignment="Left"
                                    Label="Total cruzado"
                                    LabelPosition="Above">
                                    <Label Content="{Binding TotalOrdenesCruzadas, StringFormat='\{0:N4\}', Source={StaticResource VMOrdenes}}" />
                                </Telerik:DataFormDataField>

                                <Telerik:DataFormDataField
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    HorizontalAlignment="Right"
                                    Label="Pendiente por cruzar"
                                    LabelPosition="Above">
                                    <Label Content="{Binding TotalPendientePorCruzar, StringFormat='\{0:N4\}', Source={StaticResource VMOrdenes}}" />
                                </Telerik:DataFormDataField>
                            </Grid>
                        </StackPanel>
                    </TabItem>

                </TabControl>
            </Border>

            <Border
                Grid.Row="4"
                Grid.Column="0"
                Margin="0,10,0,0"
                Style="{StaticResource Contenedor_TabControl}"
                Visibility="{Binding MostrarControles, Source={StaticResource VMOrdenes}}">
                <TabControl
                    x:Name="TabControlControles"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch">

                    <TabItem
                        x:Name="TabSaldoCliente"
                        Header="{Binding TextoSaldoPortafolio, Source={StaticResource VMOrdenes}}"
                        Visibility="{Binding MostrarSaldoPortafolioTTV, Source={StaticResource VMOrdenes}, Converter={StaticResource InVisibilityConverter}}">
                        <StackPanel
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Orientation="Vertical">
                            <!--  Control para mostrar el saldo del cliente  -->
                            <OYDPLUSControles:SaldoClienteView
                                CodigoOYDSeleccionado="{Binding CodigoOYDControles, Source={StaticResource VMOrdenes}}"
                                ConsultarSaldo="{Binding ConsultarSaldo, Source={StaticResource VMOrdenes}}"
                                MostrarExpanderAbierto="True"
                                Visibility="{Binding MostrarCamposCompra, Source={StaticResource VMOrdenes}}" />
                            <!--  Control para mostrar el portafolio del cliente  -->
                            <OYDPLUSControles:PortafolioClienteView
                                CodigoOYDSeleccionado="{Binding CodigoOYDControles, Source={StaticResource VMOrdenes}}"
                                ConsultarPortafolio="{Binding ConsultarPortafolio, Source={StaticResource VMOrdenes}}"
                                EspecieSeleccionada="{Binding EspecieControles, Source={StaticResource VMOrdenes}}"
                                PortafolioCliente="{Binding PortafolioClienteSelected, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                PortafolioSeleccionada="{Binding PortafolioSeleccionada, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                TipoNegocio="{Binding TipoNegocioControles, Source={StaticResource VMOrdenes}}"
                                Visibility="{Binding MostrarCamposVenta, Source={StaticResource VMOrdenes}}" />
                        </StackPanel>
                    </TabItem>

                    <TabItem
                        x:Name="TabSaldoClientes"
                        Header="Saldo"
                        Visibility="{Binding MostrarSaldoPortafolioTTV, Source={StaticResource VMOrdenes}, Converter={StaticResource VisibilityConverter}}">
                        <StackPanel
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Orientation="Vertical">
                            <!--  Control para mostrar el saldo del cliente  -->
                            <OYDPLUSControles:SaldoClienteView
                                CodigoOYDSeleccionado="{Binding CodigoOYDControles, Source={StaticResource VMOrdenes}}"
                                ConsultarSaldo="{Binding ConsultarSaldo, Source={StaticResource VMOrdenes}}"
                                MostrarExpanderAbierto="True" />
                        </StackPanel>
                    </TabItem>
                    <!--  ===========================================================================================================================  -->
                    <TabItem
                        x:Name="TabPortafolioCliente"
                        Header="Portafolio"
                        Visibility="{Binding MostrarSaldoPortafolioTTV, Source={StaticResource VMOrdenes}, Converter={StaticResource VisibilityConverter}}">
                        <StackPanel
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Orientation="Vertical">
                            <!--  Control para mostrar el portafolio del cliente  -->
                            <OYDPLUSControles:PortafolioClienteView
                                CodigoOYDSeleccionado="{Binding CodigoOYDControles, Source={StaticResource VMOrdenes}}"
                                ConsultarPortafolio="{Binding ConsultarPortafolio, Source={StaticResource VMOrdenes}}"
                                EspecieSeleccionada="{Binding EspecieControles, Source={StaticResource VMOrdenes}}"
                                PortafolioCliente="{Binding PortafolioClienteSelected, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                PortafolioSeleccionada="{Binding PortafolioSeleccionada, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                TipoNegocio="{Binding TipoNegocioControles, Source={StaticResource VMOrdenes}}" />
                        </StackPanel>
                    </TabItem>

                    <TabItem
                        x:Name="TabOperacionesXCumplir"
                        Header="Operaciones"
                        Visibility="{Binding MostrarCamposCuentaPropia, Source={StaticResource VMOrdenes}}">
                        <!--  Control para mostrar las operaciones pendientes por cumplir  -->
                        <OYDPLUSControles:OperacionesXCumplirView
                            CodigoOYDSeleccionado="{Binding CodigoOYDControles, Source={StaticResource VMOrdenes}}"
                            ConsultarOperaciones="{Binding ConsultarOperaciones, Source={StaticResource VMOrdenes}}"
                            EspecieSeleccionada="{Binding EspecieControles, Source={StaticResource VMOrdenes}}"
                            OperacionSeleccionada="{Binding OperacionSeleccionada, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                            OperacionXCumplirSeleccionada="{Binding OperacionesXCumplirSelected, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                            TipoNegocio="{Binding TipoNegocioControles, Source={StaticResource VMOrdenes}}"
                            TipoOperacion="{Binding TipoOperacionControles, Source={StaticResource VMOrdenes}}" />
                    </TabItem>

                    <TabItem
                        x:Name="TabOrdenSAE"
                        Header="Bolsa"
                        Visibility="{Binding MostrarOrdenesSAE, Source={StaticResource VMOrdenes}}">
                        <Telerik:RadBusyIndicator
                            x:Name="myBusyCalculando2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            BusyContent="Calculando..."
                            DisplayAfter="0"
                            IsBusy="{Binding IsBusyCalculos, Source={StaticResource VMOrdenes}}">

                            <Grid
                                x:Name="LayoutRootSAE"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">

                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>

                                <StackPanel
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Orientation="Vertical">

                                    <StackPanel Orientation="Horizontal">

                                        <TextBlock
                                            Margin="5,0,5,0"
                                            HorizontalAlignment="Center"
                                            Text="Ordenes en bolsa" />

                                        <Button
                                            x:Name="btnRefrescar"
                                            Width="15"
                                            Height="15"
                                            Padding="0"
                                            HorizontalAlignment="Center"
                                            Click="btnRefrescar_Click"
                                            Style="{StaticResource btnRefrescar}"
                                            ToolTipService.ToolTip="Refrescar ordenes" />

                                        <TextBlock
                                            Margin="10,0,0,0"
                                            Foreground="Gray"
                                            Text="Consultando ordenes..."
                                            Visibility="{Binding MostrarConsultandoOrden, Source={StaticResource VMOrdenes}}" />
                                    </StackPanel>

                                    <Grid Margin="10,0,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>

                                        <CheckBox
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Content="Seleccionar Todos"
                                            IsChecked="{Binding SeleccionarTodosFolios, Mode=TwoWay, Source={StaticResource VMOrdenes}}"
                                            IsEnabled="{Binding HabilitarSeleccionarTodosFolios, Source={StaticResource VMOrdenes}}" />

                                        <Border
                                            Grid.Column="1"
                                            Margin="10,0,0,0"
                                            Style="{StaticResource Contenedor_Paginador}"
                                            Visibility="{Binding MostrarCamposAcciones, Source={StaticResource VMOrdenes}}">
                                            <A2UTILGEN:A2DataPager
                                                x:Name="datapagerAcciones"
                                                PageSize="5"
                                                Source="{Binding Items, ElementName=dgAcciones}" />
                                        </Border>

                                        <Border
                                            Grid.Column="1"
                                            Margin="10,0,0,0"
                                            Style="{StaticResource Contenedor_Paginador}"
                                            Visibility="{Binding MostrarCamposRentaFija, Source={StaticResource VMOrdenes}}">
                                            <A2UTILGEN:A2DataPager
                                                x:Name="datapagerRentaFija"
                                                PageSize="5"
                                                Source="{Binding Items, ElementName=dgRentaFija}" />
                                        </Border>

                                    </Grid>

                                    <A2UTILGEN:A2GridView
                                        x:Name="dgAcciones"
                                        Width="auto"
                                        Height="160"
                                        AutoGenerateColumns="False"
                                        CanUserInsertRows="False"
                                        ItemsSource="{Binding ListaOrdenesSAEAccionesPaged, Source={StaticResource VMOrdenes}}"
                                        ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                                        SelectedItem="{Binding OrdenSAEAccionesSelected, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                        ValidatesOnDataErrors="None"
                                        Visibility="{Binding MostrarCamposAcciones, Source={StaticResource VMOrdenes}}">
                                        <A2UTILGEN:A2GridView.Columns>

                                            <Telerik:GridViewDataColumn Header="">
                                                <Telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding Seleccionada, Mode=TwoWay}" />
                                                    </DataTemplate>
                                                </Telerik:GridViewDataColumn.CellTemplate>
                                            </Telerik:GridViewDataColumn>

                                            <Telerik:GridViewDataColumn
                                                Width="100"
                                                DataMemberBinding="{Binding NroLiquidacion}"
                                                Header="Referencia bolsa"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="140"
                                                DataMemberBinding="{Binding FechaReferencia, StringFormat='\{0:dd/MM/yyyy\}', ConverterCulture=es-CO}"
                                                Header="Fecha"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="150"
                                                DataMemberBinding="{Binding Especie}"
                                                Header="Especie"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="110"
                                                DataMemberBinding="{Binding Cantidad, StringFormat='\{0:n2\}'}"
                                                Header="Cantidad"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="110"
                                                DataMemberBinding="{Binding Precio, StringFormat='\{0:n2\}'}"
                                                Header="Precio"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="110"
                                                DataMemberBinding="{Binding NombreEjecucion}"
                                                Header="Ejecución"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="110"
                                                DataMemberBinding="{Binding NombreDuracion}"
                                                Header="Duración"
                                                IsReadOnly="True" />
                                        </A2UTILGEN:A2GridView.Columns>
                                    </A2UTILGEN:A2GridView>


                                    <A2UTILGEN:A2GridView
                                        x:Name="dgRentaFija"
                                        Height="160"
                                        AutoGenerateColumns="False"
                                        CanUserInsertRows="False"
                                        ItemsSource="{Binding ListaOrdenesSAERentaFijaPaged, Source={StaticResource VMOrdenes}}"
                                        ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                                        SelectedItem="{Binding OrdenSAERentaFijaSelected, Source={StaticResource VMOrdenes}, Mode=TwoWay}"
                                        ValidatesOnDataErrors="None"
                                        Visibility="{Binding MostrarCamposRentaFija, Source={StaticResource VMOrdenes}}">
                                        <A2UTILGEN:A2GridView.Columns>
                                            <Telerik:GridViewDataColumn Header="">
                                                <Telerik:GridViewDataColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox IsChecked="{Binding Seleccionada, Mode=TwoWay}" />
                                                    </DataTemplate>
                                                </Telerik:GridViewDataColumn.CellTemplate>
                                            </Telerik:GridViewDataColumn>
                                            <Telerik:GridViewDataColumn
                                                Width="100"
                                                DataMemberBinding="{Binding NroLiquidacion}"
                                                Header="Referencia bolsa"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="140"
                                                DataMemberBinding="{Binding FechaReferencia, StringFormat='\{0:dd/MM/yyyy\}', ConverterCulture=es-CO}"
                                                Header="Fecha"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="100"
                                                DataMemberBinding="{Binding Especie}"
                                                Header="Especie"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="100"
                                                DataMemberBinding="{Binding Cantidad, StringFormat='\{0:n2\}'}"
                                                Header="Cantidad"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="80"
                                                DataMemberBinding="{Binding PrecioSucio, StringFormat='\{0:n2\}'}"
                                                Header="Precio"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="140"
                                                DataMemberBinding="{Binding FechaCumplimiento, StringFormat='\{0:dd/MM/yyyy\}', ConverterCulture=es-CO}"
                                                Header="Fecha cumplimiento"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="140"
                                                DataMemberBinding="{Binding FechaEmision, StringFormat='\{0:dd/MM/yyyy\}', ConverterCulture=es-CO}"
                                                Header="Fecha emisión"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="140"
                                                DataMemberBinding="{Binding FechaVencimiento, StringFormat='\{0:dd/MM/yyyy\}', ConverterCulture=es-CO}"
                                                Header="Fecha vencimiento"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="40"
                                                DataMemberBinding="{Binding DiasCumplimiento}"
                                                Header="Días"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="80"
                                                DataMemberBinding="{Binding Precio, StringFormat='{}{0}%'}"
                                                Header="Tasa"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                DataMemberBinding="{Binding NombreIndicador}"
                                                Header="Indicador"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                DataMemberBinding="{Binding Modalidad}"
                                                Header="Modalidad"
                                                IsReadOnly="True" />
                                            <Telerik:GridViewDataColumn
                                                Width="60"
                                                DataMemberBinding="{Binding UBICACIONTITULO}"
                                                Header="Depósito"
                                                IsReadOnly="True" />
                                        </A2UTILGEN:A2GridView.Columns>
                                    </A2UTILGEN:A2GridView>
                                </StackPanel>
                            </Grid>
                        </Telerik:RadBusyIndicator>
                    </TabItem>

                </TabControl>
            </Border>

        </Grid>
    </StackPanel>
</UserControl>
