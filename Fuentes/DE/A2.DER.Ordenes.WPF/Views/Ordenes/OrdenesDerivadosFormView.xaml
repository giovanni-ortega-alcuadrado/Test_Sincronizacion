<Window x:Class="OrdenesDerivadosFormView"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:A2Utils="clr-namespace:A2ControlMenu;assembly=A2ControlMenu.WPF"
        xmlns:src="clr-namespace:A2DEROrdenes"
        xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
        xmlns:Telerik="http://schemas.telerik.com/2008/xaml/presentation"
        WindowStartupLocation="CenterScreen"
        WindowStyle="ToolWindow"
        SizeToContent="WidthAndHeight"
        MinHeight="200"
        MinWidth="800">

    <Window.Resources>
        <src:VisibilityConverter x:Key="VisibilityConverter" />
        <src:InVisibilityConverter x:Key="InVisibilityConverter" />
        <src:BooleanNumeroConverter x:Key="BooleanConverter" />
    </Window.Resources>

    <Telerik:RadBusyIndicator IsBusy="{Binding IsBusy}"  DisplayAfter="0" BusyContent="Cargando..." x:Name="myBusyIndicator">
        <Grid x:Name="LayoutRoot"
              Margin="5"
              Background="{StaticResource BackgroundColorBrush}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <TextBlock Text="{Binding DataContext.DiccionarioEtiquetasPantalla[TITULO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}" Style="{StaticResource Title1}" 
                           Grid.Column="0"/>

                <StackPanel Orientation="Horizontal"
                            HorizontalAlignment="Right"
                            Grid.Column="1">
                    <A2Utils:ControlNavegacionA2 x:Name="cm"
                                                 ObjetoSeleccionado="{Binding EncabezadoEdicionSeleccionado}" 
                                                 VerticalAlignment="Center"
                                                 Visibility="{Binding Editando, Converter={StaticResource InVisibilityConverter}}"
                                                 Margin="0,0,5,0"/>
                    <Button x:Name="btnNuevoRegistro"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_NUEVO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_NUEVO].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnNuevoRegistro_Click"
                            Visibility="{Binding Editando, Converter={StaticResource InVisibilityConverter}}"
                            IsEnabled="{Binding HabilitarBotonesAcciones}"/>
                    <Button x:Name="btnEditarRegistro"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_EDITAR].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_EDITAR].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnEditarRegistro_Click"
                            Visibility="{Binding Editando, Converter={StaticResource InVisibilityConverter}}"
                            IsEnabled="{Binding HabilitarBotonesAcciones}"/>
                    <Button x:Name="btnBorrarRegistro"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_BORRAR].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_BORRAR].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnBorrarRegistro_Click"
                            Visibility="{Binding Editando, Converter={StaticResource InVisibilityConverter}}"
                            IsEnabled="{Binding HabilitarBotonesAcciones}"/>

                    <Button x:Name="btnGuardarSalir"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_GUARDARYCERRAR].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_GUARDARYCERRAR].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnGuardar_Click"
                            Visibility="{Binding Editando, Converter={StaticResource VisibilityConverter}}"/>
                    <Button x:Name="btnGuardarContinuar"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_GUARDARYCOPIARANTERIOR].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_GUARDARYCOPIARANTERIOR].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnGuardarContinuar_Click"
                            Visibility="{Binding HabilitarBoton_GuardarYCopiarAnterior, Converter={StaticResource VisibilityConverter}}"/>
                    <Button x:Name="btnGuardarContinuarNuevo"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_GUARDARYCREARNUEVO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_GUARDARYCREARNUEVO].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnGuardarContinuarNuevo_Click"
                            Visibility="{Binding HabilitarBoton_GuardarYCrearNuevo, Converter={StaticResource VisibilityConverter}}"/>
                    <Button x:Name="btnCancelar"
                            Content="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_CANCELAR].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                            ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[GENERICO_BOTON_CANCELAR].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                            Click="btnCancelar_Click"
                            Visibility="{Binding Editando, Converter={StaticResource VisibilityConverter}}"/>

                    <A2Utils:ControlNotificacionInconsistencias TooltipBotonAdvertencia="{Binding DataContext.TituloInconsistencias, RelativeSource={RelativeSource AncestorType=Window}}" 
                                                                EventoVisualizacionErrores="ControlNotificacionInconsistencias_EventoVisualizacionErrores"
                                                                Visibility="{Binding Editando, Converter={StaticResource VisibilityConverter}}"/>
                </StackPanel>
            </Grid>

            <Grid ShowGridLines="False" Background="{StaticResource BackgroundColorBrush}"
                  DataContext="{Binding EncabezadoEdicionSeleccionado}"
                  Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[ID].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[ID].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[ID].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="0" Grid.Row="0">
                    <TextBox TextAlignment="Left"
                             Text="{Binding intIdOrden}"
                             IsEnabled="False"
                             BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[ID].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                             />
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[CUENTACRCC].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[CUENTACRCC].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[CUENTACRCC].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="1" Grid.Row="0" >
                    <ComboBox ItemsSource="{Binding DataContext.DiccionarioCombosPantalla[CLIENTESCUENTASCRCC], RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding intIdCuentaCRCC, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[CUENTACRCC].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[SUBCUENTACRCC].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[SUBCUENTACRCC].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[SUBCUENTACRCC].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="2" Grid.Row="0" >
                    <ComboBox ItemsSource="{Binding DataContext.ListaSubCuentaCRCC, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding intIdSubcuentaCRCC, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[SUBCUENTACRCC].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOINSTRUMENTO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOINSTRUMENTO].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOINSTRUMENTO].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="0" Grid.Row="1" >
                    <ComboBox ItemsSource="{Binding DataContext.DiccionarioCombosPantalla[TIPOPRODUCTO], RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding DataContext.intIdTipoProducto, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOINSTRUMENTO].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectionChanged="TipoProducto_SelectionChanged">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPO].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPO].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="1" Grid.Row="1" >
                    <ComboBox ItemsSource="{Binding DataContext.ListaTipoOpcionProducto, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding intIdTipoOpcionProducto, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPO].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectionChanged="TipoOpcionProducto_SelectionChanged">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[INSTRUMENTO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[INSTRUMENTO].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[INSTRUMENTO].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="2" Grid.Row="1" >
                    <ComboBox ItemsSource="{Binding DataContext.ListaProducto, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding DataContext.intIdProducto, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[INSTRUMENTO].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOINICIAL].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOINICIAL].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOINICIAL].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="0" Grid.Row="2" >
                    <ComboBox ItemsSource="{Binding DataContext.ListaProductoVencimientos, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding intIdProductoVencimiento, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOINICIAL].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOFINAL].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOFINAL].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOFINAL].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="1" Grid.Row="2" >
                    <ComboBox ItemsSource="{Binding DataContext.ListaProductoVencimientos, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding intIdProductoEspecial, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Habilitar_VencimientoFinal, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[VENCIMIENTOFINAL].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[NOMINAL].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[NOMINAL].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[NOMINAL].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="2" Grid.Row="2">
                    <TextBox TextAlignment="Right"
                             Text="{Binding DataContext.dblNominal, RelativeSource={RelativeSource AncestorType=Window}}"
                             IsEnabled="False"
                             BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[NOMINAL].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                             />
                </Telerik:DataFormDataField>


                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOPRIMA].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOPRIMA].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOPRIMA].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="0" Grid.Row="3" >
                    <ComboBox ItemsSource="{Binding DataContext.DiccionarioCombosPantalla[TIPOPRIMA], RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding intIdTipoPrima, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Habilitar_TipoPrima, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[TIPOPRIMA].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIOPRIMA].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIOPRIMA].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIOPRIMA].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="1" Grid.Row="3" >
                    <TextBox TextAlignment="Right"
                                                 MaxLength="50"
                                                 Text="{Binding numPrecioPrima, Mode=TwoWay}"
                                                 IsEnabled="{Binding DataContext.Habilitar_PrecioPrima, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 GotFocus="seleccionarFocoControl"
                                                 BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIOPRIMA].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 KeyDown="SoloNumeros_KeyDown">
                    </TextBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[PRIMA].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[PRIMA].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[PRIMA].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="2" Grid.Row="3" >
                    <TextBox TextAlignment="Right"
                                                 MaxLength="50"
                                                 Text="{Binding numPrima, Mode=TwoWay}"
                                                 IsEnabled="{Binding DataContext.Habilitar_Prima, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 GotFocus="seleccionarFocoControl"
                                                 BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[PRIMA].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 KeyDown="SoloNumeros_KeyDown">
                    </TextBox>
                </Telerik:DataFormDataField>


                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIO].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                                               LabelPosition="Above"
                                                               ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIO].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                                               Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIO].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 Visibility="{Binding DataContext.MostrarCamposPrecio,RelativeSource={RelativeSource AncestorType=Window}}"
                                                               Grid.Column="0" Grid.Row="4">
                    <TextBox TextAlignment="Right"
                                                 MaxLength="6"
                                                 Text="{Binding numPrecio, Mode=TwoWay}"
                                                 IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 GotFocus="seleccionarFocoControl"
                                                 BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[PRECIO].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 KeyDown="SoloNumeros_KeyDown">
                    </TextBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[SPREAD].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                                               LabelPosition="Above"
                                                               ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[SPREAD].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                                               Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[SPREAD].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 Visibility="{Binding DataContext.MostrarCamposSpread,RelativeSource={RelativeSource AncestorType=Window}}"
                                                               Grid.Column="0" Grid.Row="4" >
                    <TextBox TextAlignment="Right"
                                                 MaxLength="6"
                                                 Text="{Binding numPrecio, Mode=TwoWay}"
                                                 IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 GotFocus="seleccionarFocoControl"
                                                 BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[SPREAD].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 KeyDown="SoloNumeros_KeyDown">
                    </TextBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[CANTIDADCONTRATOS].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                                               LabelPosition="Above"
                                                               ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[CANTIDADCONTRATOS].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                                               Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[CANTIDADCONTRATOS].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                                               Grid.Column="1" Grid.Row="4" >
                    <TextBox TextAlignment="Right"
                                                 MaxLength="6"
                                                 Text="{Binding numCantidad, Mode=TwoWay}"
                                                 IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 GotFocus="seleccionarFocoControl"
                                                 BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[CANTIDADCONTRATOS].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 KeyDown="SoloNumeros_KeyDown">
                    </TextBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISIONPOR].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISIONPOR].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISIONPOR].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="2" Grid.Row="4" >
                    <ComboBox ItemsSource="{Binding DataContext.DiccionarioCombosPantalla[COMISIONPOR], RelativeSource={RelativeSource AncestorType=Window}}"
                                                  SelectedValue="{Binding bitComisionPorPorcentaje, Converter={StaticResource BooleanConverter}, Mode=TwoWay}"
                                                  SelectedValuePath="Retorno"
                                                  DisplayMemberPath="Descripcion"
                                                  IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                  BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISIONPOR].Color, RelativeSource={RelativeSource AncestorType=Window}}">
                    </ComboBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISION].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
                                           LabelPosition="Above"
                                           ToolTipService.ToolTip="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISION].Tooltip, RelativeSource={RelativeSource AncestorType=Window}}" 
                                           Foreground="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISION].ColorLetra, RelativeSource={RelativeSource AncestorType=Window}}"
                                           Grid.Column="0" Grid.Row="5" >
                    <TextBox TextAlignment="Right"
                                                 MaxLength="50"
                                                 Text="{Binding numComision, Mode=TwoWay}"
                                                 IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 GotFocus="seleccionarFocoControl"
                                                 BorderBrush="{Binding DataContext.DiccionarioEtiquetasPantalla[COMISION].Color, RelativeSource={RelativeSource AncestorType=Window}}"
                                                 KeyDown="SoloNumeros_KeyDown">
                    </TextBox>
                </Telerik:DataFormDataField>

                <Telerik:DataFormDataField Label="{Binding DataContext.DiccionarioEtiquetasPantalla[FACTURARCOMISION].Titulo, RelativeSource={RelativeSource AncestorType=Window}}"
