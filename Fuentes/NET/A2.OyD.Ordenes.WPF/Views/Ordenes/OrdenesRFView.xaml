                                                                        <Style TargetType="Telerik:GridViewCell">
                                                                            <Setter Property="HorizontalAlignment" Value="Right" />
                                                                        </Style>
                                                                    </Telerik:GridViewDataColumn.CellStyle>
                                                                </Telerik:GridViewDataColumn>
                                                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Retencion, StringFormat='N2'}" Header="Retención">
                                                                    <Telerik:GridViewDataColumn.CellStyle>
                                                                        <Style TargetType="Telerik:GridViewCell">
                                                                            <Setter Property="HorizontalAlignment" Value="Right" />
                                                                        </Style>
                                                                    </Telerik:GridViewDataColumn.CellStyle>
                                                                </Telerik:GridViewDataColumn>
                                                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Intereses, StringFormat='N2'}" Header="Intereses">
                                                                    <Telerik:GridViewDataColumn.CellStyle>
                                                                        <Style TargetType="Telerik:GridViewCell">
                                                                            <Setter Property="HorizontalAlignment" Value="Right" />
                                                                        </Style>
                                                                    </Telerik:GridViewDataColumn.CellStyle>
                                                                </Telerik:GridViewDataColumn>
                                                            </A2UTILGEN:A2GridView.Columns>
                                                        </A2UTILGEN:A2GridView>
                                                    </Telerik:RadBusyIndicator>
                                                </Telerik:RadTabItem>

                                                <Telerik:RadTabItem Name="TabCuentasDeceval" Header="Cuentas Depósito">
                                                    <A2UTILGEN:A2GridView
                                                        x:Name="dgCuentasDecevalOrden"
                                                        Height="150"
                                                        HorizontalAlignment="Stretch"
                                                        VerticalAlignment="Top"
                                                        AutoGenerateColumns="False"
                                                        FrozenColumnCount="1"
                                                        IsHitTestVisible="{Binding EditandoRegistro}"
                                                        IsReadOnly="True"
                                                        ItemsSource="{Binding DataContext.CuentaDepositoDeceval, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=OneWay}"
                                                        ScrollViewer.HorizontalScrollBarVisibility="Visible"
                                                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                        SelectedItem="{Binding DataContext.LiqAsociadasOrdenSelected, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                        ValidatesOnDataErrors="None">
                                                        <A2UTILGEN:A2GridView.Columns>
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding NroCuentaDeposito, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Nro Cuenta" />
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding Conector1, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Conector" />
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding NombreBeneficiario1, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Nombre Beneficiario 1" />
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding NroDocBeneficiario1, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Nro Documento Beneficiario 1" />
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding Conector2, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Conector" />
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding NombreBeneficiario2, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Nombre Beneficiario 2" />
                                                            <Telerik:GridViewDataColumn DataMemberBinding="{Binding NroDocBeneficiario2, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" Header="Nro Documento Beneficiario 2" />
                                                        </A2UTILGEN:A2GridView.Columns>
                                                    </A2UTILGEN:A2GridView>
                                                </Telerik:RadTabItem>

                                            </Telerik:RadTabControl>

                                        </Grid>

                                        <!--  Grid Instrucciones y Notas  -->
                                        <Grid Grid.Row="3" Grid.ColumnSpan="3">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="*" />
                                            </Grid.ColumnDefinitions>

                                            <Telerik:DataFormDataField Grid.Column="0" Label="Instrucciones">
                                                <TextBox
                                            Height="Auto"
                                            MaxHeight="35"
                                            IsEnabled="{Binding DataContext.EditandoRegistro, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                            MaxLength="255"
                                            Text="{Binding Instrucciones, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                            TextWrapping="Wrap"
                                            ToolTipService.ToolTip="Instrucciones de la orden"
                                            VerticalScrollBarVisibility="Auto" />
                                            </Telerik:DataFormDataField>

                                            <Telerik:DataFormDataField Grid.Column="1" Label="Notas">
                                                <TextBox
                                            Height="Auto"
                                            MaxHeight="35"
                                            IsEnabled="{Binding DataContext.EditandoRegistro, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                            MaxLength="255"
                                            Text="{Binding Notas, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                            TextWrapping="Wrap"
                                            ToolTipService.ToolTip="Observaciones a la orden"
                                            VerticalScrollBarVisibility="Auto" />
                                            </Telerik:DataFormDataField>

                                        </Grid>

                                    </Grid>
                                </Border>
                                
                            </Grid>
                        </StackPanel>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
    </Telerik:RadBusyIndicator>
</UserControl>
