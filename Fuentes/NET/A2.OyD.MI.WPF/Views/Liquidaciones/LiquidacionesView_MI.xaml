<UserControl
    x:Class="LiquidacionesView_MI"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:A2="clr-namespace:A2ControlMenu;assembly=A2ControlMenu.WPF"
    xmlns:A2UTILGEN="clr-namespace:A2Utilidades;assembly=A2.Controles.Utilidades.WPF"
    xmlns:Telerik="http://schemas.telerik.com/2008/xaml/presentation"
    xmlns:my1="clr-namespace:A2ComunesControl;assembly=A2.OYD.Control.WPF"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:src="clr-namespace:A2OYDMI"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    MinWidth="800"
    MaxWidth="1000">

    <UserControl.Resources>
        <src:DisplayNameConverter x:Key="DisplayConverter" />
        <src:VisibilityConverter x:Key="VisibilityConverter" />
        <src:A2UtilsViewModel x:Key="A2VM" />
        <src:StringConverter x:Key="StringConverter" />
        <src:BooleanoConverterRadio x:Key="BooleanoConverterRadio" />
    </UserControl.Resources>

    <Telerik:RadBusyIndicator
        x:Name="myBusyIndicator"
        BusyContent="Cargando..."
        DisplayAfter="0"
        IsBusy="{Binding IsBusy}">
        <Grid x:Name="LayoutRoot" ShowGridLines="False">

            <Grid.Resources />
            <Grid.RowDefinitions>
                <RowDefinition Height="auto" />
                <!--  Fila del menu  -->
                <RowDefinition Height="auto" />
                <!--  Fila de la forma/grid  -->
                <RowDefinition Height="Auto" MinHeight="43" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="auto" />
                <ColumnDefinition Width="auto" />
            </Grid.ColumnDefinitions>

            <StackPanel
                Grid.Column="1"
                Orientation="Horizontal"
                Style="{StaticResource stpHead}">
                <TextBlock Style="{StaticResource txbNumRegistros}" Visibility="{Binding OrdenSelected.HasChanges, Converter={StaticResource VisibilityConverter}}" />
            </StackPanel>

            <StackPanel
                Grid.Row="0"
                Grid.Column="0"
                Style="{StaticResource stpFiltro}">
                <!--  Editando="{Binding Editando, Mode= TwoWay}"  -->
                <A2:ControlMenuA2
                    x:Name="cm"
                    DF="{Binding df}"
                    DescripcionFiltro="{Binding DescripcionFiltroVM, Mode=TwoWay}"
                    DicBotonesMenu="{Binding DicBotonesMenuVM, Mode=TwoWay}"
                    EstadoMenu="{Binding EstadoMenuVM, Mode=TwoWay}"
                    EventoCancelarGrabacion="CancelarEditarRegistro_Click"
                    EventoConfirmarGrabacion="cm_EventoConfirmarGrabacion"
                    Filtro="{Binding FiltroVM, Mode=TwoWay}"
                    IsItemValid="True"
                    ItemGridHyperlink="{Binding ItemSeleccionadoHyperlink, Mode=TwoWay}"
                    Mensaje="{Binding MensajeMVVM, Mode=TwoWay}"
                    MensajeError="{Binding ErrorForma}"
                    Modo="{Binding ModoMVVM, Mode=TwoWay}"
                    ObjetoSeleccionado="{Binding LiquidacioneSelected}"
                    TextoFiltroTooltip="Filtra por los campos Liquidación  y Comitente"
                    TituloVista="Liquidaciones Mercado Integrado"
                    TituloVistaAdicional="registros"
                    TituloVista_CambioSelected="{Binding LiquidacioneSelected.HasChanges}"
                    TituloVista_CantidadRegistros="{Binding ListaLiquidaciones.Count}" />
            </StackPanel>
            <Border Grid.Row="1" Style="{StaticResource Contenedor_Estilo}">

                <Grid
                    x:Name="grdGridForma"
                    Grid.Row="2"
                    Grid.Column="1">


                    <Border
                        Width="900"
                        HorizontalAlignment="Center"
                        CornerRadius="5">
                        <StackPanel
                            x:Name="Buscar"
                            Grid.Column="0"
                            Width="Auto"
                            Height="Auto"
                            VerticalAlignment="Top">
                            <Border Style="{StaticResource Contenedor_Paginador}">
                                <A2UTILGEN:A2DataPager
                                    x:Name="datapager1"
                                    PageSize="10"
                                    Source="{Binding Items, ElementName=dg}" />
                            </Border>
                            <A2UTILGEN:A2GridView
                                x:Name="dg"
                                Height="Auto"
                                VerticalAlignment="Top"
                                AutoGenerateColumns="False"
                                IsReadOnly="True"
                                ItemsSource="{Binding ListaLiquidacionesPaged}"
                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                SelectedItem="{Binding LiquidacioneSelected, Mode=TwoWay}"
                                ValidatesOnDataErrors="None">
                                <A2UTILGEN:A2GridView.Columns>

                                    <Telerik:GridViewDataColumn Header="Liquidación" SortMemberPath="ID">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                    VerticalAlignment="Center"
                                                    Command="{Binding DataContext.Hyperlink_CambiarAForma, RelativeSource={RelativeSource AncestorType=A2UTILGEN:A2GridView}}"
                                                    CommandParameter="{Binding}"
                                                    Content="{Binding ID}"
                                                    Style="{StaticResource HyperlinkLikeButton}" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  Tipo}" Header="Tipo" />-->
                                    <Telerik:GridViewDataColumn Header="Tipo">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <ComboBox
                                                    DisplayMemberPath="Descripcion"
                                                    IsEnabled="False"
                                                    ItemsSource="{Binding DiccionarioCombosA2[tipoLiq], Source={StaticResource A2VM}}"
                                                    SelectedValue="{Binding Tipo, Mode=TwoWay}"
                                                    SelectedValuePath="ID" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  ClaseOrden}" Header="Clase" />-->
                                    <Telerik:GridViewDataColumn Header="Clase">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <ComboBox
                                                    DisplayMemberPath="Descripcion"
                                                    IsEnabled="False"
                                                    ItemsSource="{Binding DiccionarioCombosA2[claseLiq], Source={StaticResource A2VM}}"
                                                    SelectedValue="{Binding ClaseOrden, Mode=TwoWay}"
                                                    SelectedValuePath="ID" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  Manual}" Header="Manual" />-->
                                    <Telerik:GridViewDataColumn Header="Manual">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox
                                                    HorizontalAlignment="Center"
                                                    IsChecked="{Binding Manual, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    IsEnabled="False" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDOrden}" Header="Orden" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDFactura}" Header="Factura">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>

                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Parcial}" Header="Parcial">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding ParcialPadre}" Header="Parcial Padre">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding DiasTramo}" Header="Días Tramo">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Liquidacion, StringFormat='MMM dd,yyyy'}" Header="Liquidación" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Cumplimiento, StringFormat='MMM dd,yyyy'}" Header="Cumplimiento" />
                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  Vendido}" Header="Vendido" />-->
                                    <Telerik:GridViewDataColumn Header="Vendido">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox
                                                    HorizontalAlignment="Center"
                                                    IsChecked="{Binding Vendido, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    IsEnabled="False" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDComitente}" Header="Comitente" />
                                    <!--<Telerik:GridViewDataColumn   Header="Nombre Comitente" />-->
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDOrdenante}" Header="Ordenante" />
                                    <!--<Telerik:GridViewDataColumn   Header="Nombre ordenante" />-->
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDEspecie}" Header="Especie" />
                                    <!--<Telerik:GridViewDataColumn   Header="Nombre Especie" />-->
                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  Aplazamiento}" Header="Aplazamiento" />-->
                                    <Telerik:GridViewDataColumn Header="Aplazamiento">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <CheckBox
                                                    HorizontalAlignment="Center"
                                                    IsChecked="{Binding Aplazamiento, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                    IsEnabled="False" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>

                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  IDBolsa}" Header="Bolsa" />-->
                                    <Telerik:GridViewDataColumn Header="Bolsa">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <ComboBox
                                                    Width="90"
                                                    DisplayMemberPath="Descripcion"
                                                    IsEnabled="False"
                                                    ItemsSource="{Binding DiccionarioCombosA2[Bolsas], Source={StaticResource A2VM}}"
                                                    SelectedValue="{Binding IDBolsa, StringFormat='#', Mode=TwoWay}"
                                                    SelectedValuePath="ID" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Plazo}" Header="Plazo">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding DiasVencimiento}" Header="Días al Vcto.">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>
                                    <!--<Telerik:GridViewDataColumn   Header="Nominal" />-->
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding TasaEfectiva}" Header="Efectiva">
                                        <Telerik:GridViewDataColumn.CellStyle>
                                            <Style TargetType="Telerik:GridViewCell">
                                                <Setter Property="HorizontalAlignment" Value="Right" />
                                            </Style>
                                        </Telerik:GridViewDataColumn.CellStyle>
                                    </Telerik:GridViewDataColumn>
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Modalidad}" Header="Modalidad" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Emision, StringFormat='MMM dd,yyyy'}" Header="Emisión" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Vencimiento, StringFormat='MMM dd,yyyy'}" Header="Vencimiento" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding Plaza}" Header="Plaza" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDComisionistaLocal}" Header="Comisionista Local" />
                                    <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDComisionistaOtraPlaza}" Header="Comisionista OP" />
                                    <!--<Telerik:GridViewDataColumn  DataMemberBinding="{Binding  IDCiudadOtraPlaza}" Header="Ciudad OP" />-->
                                    <Telerik:GridViewDataColumn Header="Cuidad OP">
                                        <Telerik:GridViewDataColumn.CellTemplate>
                                            <DataTemplate>
                                                <ComboBox
                                                    Margin="0,0,0,0"
                                                    DisplayMemberPath="Descripcion"
                                                    IsEnabled="False"
                                                    ItemsSource="{Binding DiccionarioCombosA2[CiudadesBolsas], Source={StaticResource A2VM}}"
                                                    SelectedValue="{Binding IDCiudadOtraPlaza, Converter={StaticResource StringConverter}, Mode=TwoWay}"
                                                    SelectedValuePath="ID" />
                                            </DataTemplate>
                                        </Telerik:GridViewDataColumn.CellTemplate>
                                    </Telerik:GridViewDataColumn>
                                </A2UTILGEN:A2GridView.Columns>
                            </A2UTILGEN:A2GridView>
                        </StackPanel>
                    </Border>

                    <Border
                        Width="900"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        CornerRadius="5">
                        <Grid>
                            <!--  Dataform de búsqueda  -->
                            <A2UTILGEN:A2DataForm
                                x:Name="dfBuscar"
                                VerticalAlignment="Top"
                                AutoGenerateFields="False"
                                CommandButtonsVisibility="None"
                                CurrentItem="{Binding cb}"
                                Visibility="{Binding visBuscando}">
                                <A2UTILGEN:A2DataForm.EditTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Vertical">
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                    <ColumnDefinition Width="*" />
                                                </Grid.ColumnDefinitions>
                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="0"
                                                    VerticalAlignment="Top"
                                                    Label="Orden"
                                                    LabelPosition="Above">
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <A2UTILGEN:A2NumericBox
                                                            Grid.Column="0"
                                                            Maximum="2147483647"
                                                            Minimum="0"
                                                            ShowButtons="True"
                                                            ToolTipService.ToolTip="IDOrden"
                                                            Value="{Binding NumeroOrden, Mode=TwoWay}" />
                                                        <TextBox
                                                            x:Name="txtnOrden"
                                                            Grid.Column="1"
                                                            Width="45"
                                                            Loaded="LiquidacionesBuscar_Loaded"
                                                            MaxLength="6"
                                                            Text="{Binding NumeroOrden1, Mode=TwoWay}"
                                                            ToolTipService.ToolTip="IDOrden" />
                                                    </Grid>
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="1"
                                                    VerticalAlignment="Top"
                                                    Label="Comitente"
                                                    LabelPosition="Above">
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBox
                                                            Grid.Column="0"
                                                            IsEnabled="False"
                                                            MaxLength="17"
                                                            Text="{Binding IDComitente, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                        <my1:BuscadorClienteListaButon
                                                            Grid.Column="1"
                                                            Margin="2"
                                                            Agrupamiento="todoslosreceptores"
                                                            CampoBusqueda="Nombre"
                                                            EstadoComitente="A"
                                                            TipoVinculacion="C"
                                                            finalizoBusqueda="Buscar_finalizoBusqueda" />
                                                    </Grid>
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="0"
                                                    Grid.Column="2"
                                                    VerticalAlignment="Top"
                                                    Label="Ordenante"
                                                    LabelPosition="Above">
                                                    <Grid>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="auto" />
                                                        </Grid.ColumnDefinitions>
                                                        <TextBox
                                                            Grid.Column="0"
                                                            IsEnabled="False"
                                                            MaxLength="17"
                                                            Text="{Binding IDOrdenante, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                        <my1:BuscadorClienteListaButon
                                                            Grid.Column="1"
                                                            Margin="2"
                                                            Agrupamiento="todoslosreceptores"
                                                            CampoBusqueda="NombreOrdenante"
                                                            EstadoComitente="A"
                                                            TipoVinculacion="O"
                                                            finalizoBusqueda="Buscar_finalizoBusqueda" />
                                                    </Grid>
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="1"
                                                    Grid.Column="0"
                                                    VerticalAlignment="Top"
                                                    Label="Liquidación"
                                                    LabelPosition="Above">
                                                    <A2UTILGEN:A2NumericBox
                                                        Maximum="2147483647"
                                                        NumberDecimalDigits="0"
                                                        ShowButtons="False"
                                                        ValueFormat="NoSeparator"
                                                        Value="{Binding ID, Mode=TwoWay}" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="1"
                                                    Grid.Column="1"
                                                    VerticalAlignment="Top"
                                                    Label="Parcial"
                                                    LabelPosition="Above">
                                                    <A2UTILGEN:A2NumericBox
                                                        Maximum="2147483647"
                                                        NumberDecimalDigits="0"
                                                        ShowButtons="False"
                                                        ValueFormat="NoSeparator"
                                                        Value="{Binding Parcial, Mode=TwoWay}" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="1"
                                                    Grid.Column="2"
                                                    VerticalAlignment="Top"
                                                    Label="Cumplimiento Titulo"
                                                    LabelPosition="Above">
                                                    <A2UTILGEN:A2DatePicker
                                                        x:Name="Cumplimiento"
                                                        DisplayDate="{Binding DisplayDate, Mode=OneWay}"
                                                        SelectedDate="{Binding CumplimientoTitulo, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="1"
                                                    Grid.Column="3"
                                                    VerticalAlignment="Top"
                                                    Label="Liquidación"
                                                    LabelPosition="Above">
                                                    <A2UTILGEN:A2DatePicker
                                                        x:Name="FLiquidacion"
                                                        DisplayDate="{Binding DisplayDate2, Mode=OneWay}"
                                                        SelectedDate="{Binding Liquidacion, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                </Telerik:DataFormDataField>
                                            </Grid>
                                        </StackPanel>
                                    </DataTemplate>
                                </A2UTILGEN:A2DataForm.EditTemplate>
                            </A2UTILGEN:A2DataForm>

                            <!--  Dataform edición de registro  -->
                            <StackPanel
                                x:Name="Editar"
                                Height="Auto"
                                Visibility="{Binding DataContext.visNavegando, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                <StackPanel
                                    Grid.Row="1"
                                    HorizontalAlignment="Right"
                                    VerticalAlignment="Top"
                                    Orientation="Horizontal"
                                    Style="{StaticResource stpFiltro}">

                                    <Button
                                        Width="100"
                                        HorizontalAlignment="Right"
                                        HorizontalContentAlignment="Center"
                                        Click="btnAplazar_Click"
                                        Content="Aplazar"
                                        IsEnabled="{Binding habilitaboton}"
                                        Style="{StaticResource ControlButtonSecundaryStyle}" />
                                    <Button
                                        Width="100"
                                        HorizontalAlignment="Right"
                                        HorizontalContentAlignment="Center"
                                        Click="btnDuplicar_Click"
                                        Content="Duplicar"
                                        IsEnabled="{Binding habilitaboton}"
                                        Style="{StaticResource ControlButtonSecundaryStyle}" />
                                </StackPanel>


                                <A2UTILGEN:A2DataForm
                                    x:Name="df"
                                    VerticalAlignment="Top"
                                    AutoGenerateFields="False"
                                    CommandButtonsVisibility="None"
                                    CurrentItem="{Binding LiquidacioneSelected}">
                                    <A2UTILGEN:A2DataForm.EditTemplate>
                                        <DataTemplate>
                                            <StackPanel Margin="0,0,0,0" Orientation="Vertical">
                                                <Grid Name="StackPanel2">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="70*" />
                                                        <ColumnDefinition Width="30*" />
                                                    </Grid.ColumnDefinitions>
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />
                                                    </Grid.RowDefinitions>

                                                    <Grid
                                                        Grid.Row="0"
                                                        Grid.Column="0"
                                                        IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=UserControl}}">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="*" />
                                                            <ColumnDefinition Width="*" />
                                                        </Grid.ColumnDefinitions>

                                                        <Telerik:DataFormDataField
                                                            Grid.Row="0"
                                                            Grid.RowSpan="2"
                                                            Grid.Column="0"
                                                            VerticalAlignment="Top"
                                                            IsEnabled="{Binding DataContext.habilitaradio, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                            Label="Tipos"
                                                            LabelPosition="Above">
                                                            <ListBox
                                                                BorderThickness="0"
                                                                DisplayMemberPath="Descripcion"
                                                                ItemsSource="{Binding DiccionarioCombosA2[tipoLiq], Source={StaticResource A2VM}}"
                                                                SelectedValue="{Binding Tipo, Mode=TwoWay}"
                                                                SelectedValuePath="ID">
                                                                <ListBox.ItemContainerStyle>
                                                                    <Style TargetType="ListBoxItem">
                                                                        <Setter Property="Template">
                                                                            <Setter.Value>
                                                                                <ControlTemplate TargetType="ListBoxItem">
                                                                                    <Grid Background="Transparent">
                                                                                        <RadioButton IsChecked="{TemplateBinding IsSelected}" IsHitTestVisible="False">
                                                                                            <ContentPresenter />
                                                                                        </RadioButton>
                                                                                    </Grid>
                                                                                </ControlTemplate>
                                                                            </Setter.Value>
                                                                        </Setter>
                                                                    </Style>
                                                                </ListBox.ItemContainerStyle>
                                                            </ListBox>
                                                        </Telerik:DataFormDataField>

                                                        <Telerik:DataFormDataField
                                                            Grid.Row="0"
                                                            Grid.Column="1"
                                                            IsEnabled="{Binding DataContext.habilitaradio, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                            Label="{Binding Mode=OneWay, Converter={StaticResource ResourceKey=DisplayConverter}, ConverterParameter=ClaseOrden}"
                                                            LabelPosition="Above">
                                                            <ListBox
                                                                BorderThickness="0"
                                                                DisplayMemberPath="Descripcion"
                                                                ItemsSource="{Binding DiccionarioCombosA2[claseLiq], Source={StaticResource A2VM}}"
                                                                SelectedValue="{Binding ClaseOrden, Mode=TwoWay}"
                                                                SelectedValuePath="ID">
                                                                <ListBox.ItemContainerStyle>
                                                                    <Style TargetType="ListBoxItem">
                                                                        <Setter Property="Template">
                                                                            <Setter.Value>
                                                                                <ControlTemplate TargetType="ListBoxItem">
                                                                                    <Grid Background="Transparent">
                                                                                        <RadioButton IsChecked="{TemplateBinding IsSelected}" IsHitTestVisible="False">
                                                                                            <ContentPresenter />
                                                                                        </RadioButton>
                                                                                    </Grid>
                                                                                </ControlTemplate>
                                                                            </Setter.Value>
                                                                        </Setter>
                                                                    </Style>
                                                                </ListBox.ItemContainerStyle>
                                                            </ListBox>
                                                        </Telerik:DataFormDataField>

                                                        <Telerik:DataFormDataField
                                                            Grid.Row="1"
                                                            Grid.Column="1"
                                                            IsReadOnly="True"
                                                            Label="{Binding Mode=OneWay, Converter={StaticResource ResourceKey=DisplayConverter}, ConverterParameter=Manual}"
                                                            LabelPosition="Above">
                                                            <ListBox
                                                                BorderThickness="0"
                                                                DisplayMemberPath="Descripcion"
                                                                ItemsSource="{Binding DiccionarioCombosA2[manualLiq], Source={StaticResource A2VM}}"
                                                                SelectedValue="{Binding Manual, Mode=TwoWay, Converter={StaticResource BooleanoConverterRadio}}"
                                                                SelectedValuePath="ID">
                                                                <ListBox.ItemsPanel>
                                                                    <ItemsPanelTemplate>
                                                                        <StackPanel Orientation="Horizontal" />
                                                                    </ItemsPanelTemplate>
                                                                </ListBox.ItemsPanel>
                                                                <ListBox.ItemContainerStyle>
