                            Height="Auto"
                            VerticalAlignment="Top"
                            AutoGenerateColumns="False"
                            IsReadOnly="True"
                            ItemsSource="{Binding ListaLiquidacionesOFPaged}"
                            ScrollViewer.VerticalScrollBarVisibility="Auto"
                            SelectedItem="{Binding LiquidacionesOFSelected, Mode=TwoWay}"
                            ValidatesOnDataErrors="None">
                            <A2UTILGEN:A2GridView.Columns>

                                <Telerik:GridViewDataColumn Header="Liquidación" SortMemberPath="ID">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <Button
                                                VerticalAlignment="Center"
                                                Command="{Binding DataContext.Hyperlink_CambiarAForma, RelativeSource={RelativeSource AncestorType=A2UTILGEN:A2GridView}}"
                                                CommandParameter="{Binding}"
                                                Content="{Binding ID}"
                                                Style="{StaticResource HyperlinkLikeButton}" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn Header="Tipo">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="False"
                                                ItemsSource="{Binding DiccionarioCombosA2[tipoLiq], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding Tipo, Mode=TwoWay}"
                                                SelectedValuePath="ID" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn Header="Clase">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <ComboBox
                                                DisplayMemberPath="Descripcion"
                                                IsEnabled="False"
                                                ItemsSource="{Binding DiccionarioCombosA2[claseLiq], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding ClaseOrden, Mode=TwoWay}"
                                                SelectedValuePath="ID" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn Header="Manual">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox
                                                HorizontalAlignment="Center"
                                                IsChecked="{Binding Manual, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                IsEnabled="False" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDOrden}" Header="Orden" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDFactura}" Header="Factura" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Parcial}" Header="Parcial" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding ParcialPadre}" Header="Parcial Padre" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding DiasTramo}" Header="Días Tramo" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Liquidacion, StringFormat='MMM dd,yyyy'}" Header="Liquidación" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Cumplimiento, StringFormat='MMM dd,yyyy'}" Header="Cumplimiento" />
