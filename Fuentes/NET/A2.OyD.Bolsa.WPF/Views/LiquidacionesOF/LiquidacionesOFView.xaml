                                    <Button
                                        Width="100"
                                        Click="btnDuplicar_Click"
                                        Content="Duplicar"
                                        IsEnabled="{Binding habilitaboton}"
                                        Style="{StaticResource ControlButtonSecundaryStyle}" />
                                </StackPanel>

