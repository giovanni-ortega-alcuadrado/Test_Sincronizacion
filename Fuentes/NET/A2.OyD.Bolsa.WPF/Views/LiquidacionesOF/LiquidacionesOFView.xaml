                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Modalidad}" Header="Modalidad" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Emision, StringFormat='MMM dd,yyyy'}" Header="Emisión" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Vencimiento, StringFormat='MMM dd,yyyy'}" Header="Vencimiento" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Plaza}" Header="Plaza" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDComisionistaLocal}" Header="Comisionista Local" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDComisionistaOtraPlaza}" Header="Comisionista OP" />
