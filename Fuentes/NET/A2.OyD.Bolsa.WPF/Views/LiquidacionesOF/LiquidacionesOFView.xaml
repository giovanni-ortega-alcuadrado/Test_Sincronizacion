                                                IsEnabled="False"
                                                ItemsSource="{Binding DiccionarioCombosA2[CiudadesBolsas], Source={StaticResource A2VM}}"
                                                SelectedValue="{Binding IDCiudadOtraPlaza, Converter={StaticResource StringConverter}, Mode=TwoWay}"
                                                SelectedValuePath="ID" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                            </A2UTILGEN:A2GridView.Columns>
                        </A2UTILGEN:A2GridView>
