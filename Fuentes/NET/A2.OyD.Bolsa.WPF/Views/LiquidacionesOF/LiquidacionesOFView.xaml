                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox
                                                HorizontalAlignment="Center"
                                                IsChecked="{Binding Vendido, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                IsEnabled="False" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDComitente}" Header="Comitente" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDOrdenante}" Header="Ordenante" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDEspecie}" Header="Especie" />
                                <Telerik:GridViewDataColumn Header="Aplazamiento">
                                    <Telerik:GridViewDataColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox
                                                HorizontalAlignment="Center"
                                                IsChecked="{Binding Aplazamiento, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                IsEnabled="False" />
                                        </DataTemplate>
                                    </Telerik:GridViewDataColumn.CellTemplate>
                                </Telerik:GridViewDataColumn>
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding IDBolsa}" Header="Bolsa" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding Plazo}" Header="Plazo" />
                                <Telerik:GridViewDataColumn DataMemberBinding="{Binding DiasVencimiento}" Header="Días al Vcto." />
