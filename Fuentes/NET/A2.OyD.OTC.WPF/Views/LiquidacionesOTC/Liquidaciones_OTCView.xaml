                                                    LabelPosition="Above"
                                                    TabIndex="9">
                                                    <A2UTILGEN:A2DatePicker SelectedDate="{Binding EMISION, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" ToolTipService.ToolTip="Fecha de Emisión" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="5"
                                                    Grid.Column="0"
                                                    VerticalAlignment="Top"
                                                    IsEnabled="{Binding DataContext.logHabilitaAccion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    Label="Vencimiento"
                                                    LabelPosition="Above"
                                                    TabIndex="10">
                                                    <A2UTILGEN:A2DatePicker SelectedDate="{Binding VENCIMIENTO, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" ToolTipService.ToolTip="Fecha de Vencimiento" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="6"
                                                    Grid.Column="0"
                                                    VerticalAlignment="Top"
                                                    IsEnabled="{Binding DataContext.logHabilitaAccion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    Label="Dias Vencimiento"
                                                    LabelPosition="Above"
                                                    TabIndex="11">
                                                    <A2UTILGEN:A2NumericBox
                                                        Name="Dias"
                                                        Maximum="9999"
                                                        NumberDecimalDigits="0"
                                                        ShowButtons="False"
                                                        ToolTipService.ToolTip="Numero Operación Sistema"
                                                        ValueFormat="NoSeparator"
                                                        Value="{Binding DIASALVENCIMIENTOTITULO, StringFormat=n0, Mode=TwoWay}" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="7"
                                                    Grid.Column="0"
                                                    VerticalAlignment="Top"
                                                    IsEnabled="{Binding DataContext.logHabilitaAccion, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                    Label="Modalidad"
                                                    LabelPosition="Above"
                                                    TabIndex="12">
                                                    <ComboBox
                                                        x:Name="cboModalidad"
                                                        DisplayMemberPath="Descripcion"
                                                        ItemsSource="{Binding DiccionarioCombosEspecificos[MODALIDAD], Source={StaticResource A2VM}, Mode=OneWay}"
                                                        SelectedValue="{Binding MODALIDADTASANOMINAL, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                        SelectedValuePath="ID"
                                                        ToolTipService.ToolTip="Modalidad" />
                                                </Telerik:DataFormDataField>

                                                <!--  **********************************  -->

                                                <!--  Recuadro de Tasa y Receptores  -->
                                                <Border
                                                    Grid.Row="4"
                                                    Grid.RowSpan="4"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="4"
                                                    Style="{StaticResource BordeAgrupar}">
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="Auto" />
                                                            <RowDefinition Height="Auto" />
                                                        </Grid.RowDefinitions>
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="*" />
                                                        </Grid.ColumnDefinitions>

                                                        <Grid Grid.Row="0" Grid.Column="0">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="10*" />
                                                                <ColumnDefinition Width="10*" />
                                                                <ColumnDefinition Width="10*" />
                                                                <ColumnDefinition Width="10*" />
                                                                <ColumnDefinition Width="10*" />
                                                                <ColumnDefinition Width="50*" />
                                                            </Grid.ColumnDefinitions>
                                                            <Telerik:DataFormDataField
                                                                Grid.Column="0"
                                                                Label="Tasa"
                                                                LabelPosition="Above"
                                                                TabIndex="14">
                                                                <ListBox
                                                                    BorderThickness="1"
                                                                    DisplayMemberPath="Descripcion"
                                                                    ItemsSource="{Binding DiccionarioCombosEspecificos[clase_OTC], Source={StaticResource A2VM}}"
                                                                    SelectedValue="{Binding RENTAFIJA, Converter={StaticResource BooleanoConverterRadio}, Mode=TwoWay}"
                                                                    SelectedValuePath="ID">
                                                                    <ListBox.ItemsPanel>
                                                                        <ItemsPanelTemplate>
                                                                            <StackPanel Orientation="Horizontal" />
                                                                        </ItemsPanelTemplate>
                                                                    </ListBox.ItemsPanel>
                                                                    <ListBox.ItemContainerStyle>
                                                                        <Style TargetType="ListBoxItem">
                                                                            <Setter Property="Template">
                                                                                <Setter.Value>
                                                                                    <ControlTemplate TargetType="ListBoxItem">
                                                                                        <Grid Background="Transparent">
                                                                                            <RadioButton IsChecked="{TemplateBinding IsSelected}" IsHitTestVisible="False">
                                                                                                <ContentPresenter />
                                                                                            </RadioButton>
                                                                                        </Grid>
                                                                                    </ControlTemplate>
                                                                                </Setter.Value>
                                                                            </Setter>
                                                                        </Style>
                                                                    </ListBox.ItemContainerStyle>
                                                                </ListBox>

                                                            </Telerik:DataFormDataField>

                                                            <Telerik:DataFormDataField
                                                                Grid.Column="1"
                                                                Label="Nominal"
                                                                LabelPosition="Above"
                                                                TabIndex="15">
                                                                <A2UTILGEN:A2NumericBox
                                                                    Minimum="0"
                                                                    NumberDecimalDigits="3"
                                                                    ShowButtons="False"
                                                                    ToolTipService.ToolTip="Tasa Nominal"
                                                                    ValueFormat="Numeric"
                                                                    Value="{Binding TASAINTERESNOMINAL, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                            </Telerik:DataFormDataField>

                                                            <Telerik:DataFormDataField
                                                                Grid.Column="2"
                                                                Label="Efectiva"
                                                                LabelPosition="Above"
                                                                TabIndex="16">
                                                                <A2UTILGEN:A2NumericBox
                                                                    IsEnabled="{Binding DataContext.logTasaFija, Mode=OneWay, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                    NumberDecimalDigits="3"
                                                                    ShowButtons="False"
                                                                    ToolTipService.ToolTip="Tasa Efectiva Anual"
                                                                    ValueFormat="Numeric"
                                                                    Value="{Binding TASAEFECTIVAANUAL, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                            </Telerik:DataFormDataField>

                                                            <Telerik:DataFormDataField
                                                                Grid.Column="3"
                                                                IsEnabled="{Binding DataContext.logTasaFija, Mode=OneWay, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource BooleanoConverterContrario}}"
                                                                Label="Indicador"
                                                                LabelPosition="Above"
                                                                TabIndex="17">
                                                                <ComboBox
                                                                    x:Name="cboIndicador"
                                                                    DisplayMemberPath="Descripcion"
                                                                    ItemsSource="{Binding DiccionarioCombosEspecificos[INDICADOR], Source={StaticResource A2VM}, Mode=OneWay}"
                                                                    SelectedValue="{Binding INDICADOR, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                                    SelectedValuePath="ID"
                                                                    ToolTipService.ToolTip="Indicador" />
                                                            </Telerik:DataFormDataField>

                                                            <Telerik:DataFormDataField
                                                                Grid.Column="4"
                                                                Label="Puntos Indicador"
                                                                LabelPosition="Above"
                                                                TabIndex="18">
                                                                <A2UTILGEN:A2NumericBox
                                                                    IsEnabled="{Binding DataContext.logTasaFija, Mode=OneWay, RelativeSource={RelativeSource AncestorType=UserControl}, Converter={StaticResource BooleanoConverterContrario}}"
                                                                    Maximum="99999"
                                                                    NumberDecimalDigits="0"
                                                                    ShowButtons="False"
                                                                    ToolTipService.ToolTip="Puntos Indicador"
                                                                    ValueFormat="Numeric"
                                                                    Value="{Binding PUNTOSINDICADOR, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />

                                                            </Telerik:DataFormDataField>

                                                            <A2Utils:ControlMenuDetalleA2
                                                                x:Name="cmReceptores"
                                                                Grid.Column="5"
                                                                VerticalAlignment="Bottom"
                                                                IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                Mensaje="{Binding DataContext.MensajeMVVM, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                NombreDetalle="{Binding DataContext.NombreColeccionDetalle, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                ObjetoSeleccionado="{Binding DataContext.ReceptorSelected, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                PreguntarConfirmacion="False" />

                                                        </Grid>

                                                        <!--  Grid de Receptores  -->
                                                        <StackPanel
                                                            Grid.Row="1"
                                                            Grid.RowSpan="2"
                                                            Grid.Column="0"
                                                            Orientation="Vertical">
                                                            <A2UTILGEN:A2GridView
                                                                x:Name="dgReceptores"
                                                                Height="150"
                                                                Margin="2"
                                                                VerticalAlignment="Top"
                                                                AutoGenerateColumns="False"
                                                                IsEnabled="{Binding DataContext.Editando, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                ItemsSource="{Binding DataContext.ListaReceptores, RelativeSource={RelativeSource AncestorType=UserControl}}"
                                                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                                SelectedItem="{Binding DataContext.ReceptorSelected, RelativeSource={RelativeSource AncestorType=UserControl}, Mode=TwoWay}"
                                                                ValidatesOnDataErrors="None">

                                                                <A2UTILGEN:A2GridView.Columns>

                                                                    <Telerik:GridViewDataColumn Width="170" Header="Receptor">
                                                                        <Telerik:GridViewDataColumn.CellTemplate>
                                                                            <DataTemplate>
                                                                                <StackPanel Orientation="Horizontal">
                                                                                    <TextBlock Width="145" Text="{Binding IDReceptor, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                                                    <my1:BuscadorGenericoListaButon
                                                                                        Margin="2,0,0,0"
                                                                                        Agrupamiento=""
                                                                                        CampoBusqueda="IDReceptor"
                                                                                        EstadoItem="A"
                                                                                        Etiqueta="Receptores"
                                                                                        TipoItem="Receptores"
                                                                                        finalizoBusqueda="BuscarReceptor_finalizoBusqueda" />
                                                                                </StackPanel>
                                                                            </DataTemplate>
                                                                        </Telerik:GridViewDataColumn.CellTemplate>
                                                                    </Telerik:GridViewDataColumn>

                                                                    <Telerik:GridViewDataColumn Header="Porcentaje">
                                                                        <Telerik:GridViewDataColumn.CellTemplate>
                                                                            <DataTemplate>
                                                                                <A2UTILGEN:A2NumericBox
                                                                                    Name="nbPorcentaje"
                                                                                    Maximum="100"
                                                                                    Minimum="0"
                                                                                    NumberDecimalDigits="0"
                                                                                    ShowButtons="False"
                                                                                    ValueFormat="Numeric"
                                                                                    Value="{Binding Porcentaje, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                                            </DataTemplate>
                                                                        </Telerik:GridViewDataColumn.CellTemplate>
                                                                    </Telerik:GridViewDataColumn>

                                                                    <Telerik:GridViewDataColumn Header="Lider">
                                                                        <Telerik:GridViewDataColumn.CellTemplate>
                                                                            <DataTemplate>
                                                                                <CheckBox
                                                                                    Name="chkLider"
                                                                                    HorizontalAlignment="Center"
                                                                                    VerticalAlignment="Center"
                                                                                    IsChecked="{Binding Lider, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                                            </DataTemplate>
                                                                        </Telerik:GridViewDataColumn.CellTemplate>
                                                                    </Telerik:GridViewDataColumn>
                                                                </A2UTILGEN:A2GridView.Columns>
                                                            </A2UTILGEN:A2GridView>
                                                        </StackPanel>

                                                    </Grid>

                                                </Border>
                                                <!--  ***************************************************  -->
                                                <!--  Tipo pago y Estado  -->
                                                <Telerik:DataFormDataField
                                                    Grid.Row="8"
                                                    Grid.Column="0"
                                                    VerticalAlignment="Top"
                                                    Label="Pago Operación"
                                                    LabelPosition="Above"
                                                    TabIndex="13">
                                                    <ComboBox
                                                        x:Name="cboTipoPagoOperacion"
                                                        DisplayMemberPath="Descripcion"
                                                        ItemsSource="{Binding DiccionarioCombosEspecificos[PAGOOPERACION], Source={StaticResource A2VM}, Mode=OneWay}"
                                                        SelectedValue="{Binding TIPOPAGOOPERACION, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                        SelectedValuePath="ID"
                                                        ToolTipService.ToolTip="Tipo de pago de la Operación" />
                                                </Telerik:DataFormDataField>

                                                <Telerik:DataFormDataField
                                                    Grid.Row="9"
                                                    Grid.Column="0"
                                                    IsEnabled="False"
                                                    Label="Estado"
                                                    LabelPosition="Above">
                                                    <ListBox
                                                        BorderThickness="1"
                                                        DisplayMemberPath="Descripcion"
                                                        ItemsSource="{Binding DiccionarioCombosEspecificos[ESTADO_OTC], Source={StaticResource A2VM}}"
                                                        SelectedValue="{Binding ESTADO, Mode=TwoWay}"
                                                        SelectedValuePath="ID">
                                                        <ListBox.ItemsPanel>
                                                            <ItemsPanelTemplate>
                                                                <StackPanel Orientation="Vertical" />
                                                            </ItemsPanelTemplate>
                                                        </ListBox.ItemsPanel>
                                                        <ListBox.ItemContainerStyle>
                                                            <Style TargetType="ListBoxItem">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="ListBoxItem">
                                                                            <Grid Background="Transparent">
                                                                                <RadioButton IsChecked="{TemplateBinding IsSelected}" IsHitTestVisible="False">
                                                                                    <ContentPresenter />
                                                                                </RadioButton>
                                                                            </Grid>
                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </ListBox.ItemContainerStyle>
                                                    </ListBox>
                                                </Telerik:DataFormDataField>

                                                <!--  Informacion del sistema  -->
                                                <Border
                                                    Grid.Row="8"
                                                    Grid.RowSpan="3"
                                                    Grid.Column="1"
                                                    Grid.ColumnSpan="2"
                                                    Style="{StaticResource BordeAgrupar}">
                                                    <StackPanel Orientation="Vertical">
                                                        <Telerik:DataFormDataField Label="Tipo sistema" TabIndex="19">
                                                            <ComboBox
                                                                x:Name="cboTipoSistema"
                                                                DisplayMemberPath="Descripcion"
                                                                ItemsSource="{Binding DiccionarioCombosEspecificos[OPERACION_OTC], Source={StaticResource A2VM}, Mode=OneWay}"
                                                                SelectedValue="{Binding REGISTROOPERACION, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                                SelectedValuePath="ID"
                                                                ToolTipService.ToolTip="Tipo" />
                                                        </Telerik:DataFormDataField>
                                                        <Telerik:DataFormDataField Label="Nombre sistema" TabIndex="20">
                                                            <ComboBox
                                                                x:Name="cboSistema"
                                                                DisplayMemberPath="Descripcion"
                                                                ItemsSource="{Binding DiccionarioCombosEspecificos[SISTEMA_OTC], Source={StaticResource A2VM}, Mode=OneWay}"
                                                                SelectedValue="{Binding NOMBRESISTEMA, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}"
                                                                SelectedValuePath="Descripcion"
                                                                ToolTipService.ToolTip="Nombre Sistema" />
                                                        </Telerik:DataFormDataField>

                                                        <Telerik:DataFormDataField
                                                            VerticalAlignment="Top"
                                                            Label="Operación"
                                                            TabIndex="21">
                                                            <A2UTILGEN:A2NumericBox
                                                                Name="Operacion"
                                                                Maximum="999999999"
                                                                Minimum="0"
                                                                NumberDecimalDigits="0"
                                                                ShowButtons="False"
                                                                ToolTipService.ToolTip="Numero Operación Sistema"
                                                                ValueFormat="NoSeparator"
                                                                Value="{Binding NUMEROOPERACION, StringFormat=n0, Mode=TwoWay}" />
                                                        </Telerik:DataFormDataField>

                                                    </StackPanel>
                                                </Border>

                                                <!--  Cálculos  -->
                                                <Border
                                                    Grid.Row="8"
                                                    Grid.RowSpan="3"
                                                    Grid.Column="3"
                                                    Grid.ColumnSpan="2"
                                                    Style="{StaticResource BordeAgrupar}">

                                                    <StackPanel Orientation="Vertical">

                                                        <Telerik:DataFormDataField Label="Monto" TabIndex="22">
                                                            <A2UTILGEN:A2NumericBox
                                                                Maximum="999999999999"
                                                                Minimum="0"
                                                                NumberDecimalDigits="2"
                                                                ShowButtons="False"
                                                                ToolTipService.ToolTip="Monto"
                                                                ValueFormat="Numeric"
                                                                Value="{Binding MONTO, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                        </Telerik:DataFormDataField>

                                                        <Telerik:DataFormDataField Label="Cantidad" TabIndex="23">
                                                            <A2UTILGEN:A2NumericBox
                                                                Maximum="999999999999"
                                                                Minimum="0"
                                                                NumberDecimalDigits="0"
                                                                ShowButtons="False"
                                                                ToolTipService.ToolTip="Cantidad"
                                                                ValueFormat="Numeric"
                                                                Value="{Binding CANTIDADNEGOCIADA, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                        </Telerik:DataFormDataField>

                                                        <Telerik:DataFormDataField
                                                            IsEnabled="False"
                                                            Label="Precio"
                                                            TabIndex="24">
                                                            <A2UTILGEN:A2NumericBox
                                                                NumberDecimalDigits="3"
                                                                ShowButtons="False"
                                                                ToolTipService.ToolTip="Precio"
                                                                ValueFormat="Numeric"
                                                                Value="{Binding PRECIO, Mode=TwoWay, NotifyOnValidationError=True, ValidatesOnExceptions=True}" />
                                                        </Telerik:DataFormDataField>

                                                    </StackPanel>
                                                </Border>

                                            </Grid>

                                        </DataTemplate>
                                    </A2UTILGEN:A2DataForm.EditTemplate>
                                </A2UTILGEN:A2DataForm>

                            </StackPanel>
                        </Grid>
                    </Border>
                </Grid>
            </Border>
        </Grid>
    </Telerik:RadBusyIndicator>
</UserControl>



